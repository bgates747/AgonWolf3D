04C30B             0001*  ; Table 32. Timer Control Registers
04C30B             0002*  ; this constant is the base address of the timer control registers
04C30B             0003*  ; each timer takes three bytes:
04C30B             0004*  ;   0: control register
04C30B             0005*  ;   1: low byte of timer reset value
04C30B             0006*  ;   2: high byte of timer reset value
04C30B             0007*  ; note that the value is only 8-bits, so we use in0/out0 i/o calls,
04C30B             0008*  ; which correctly force the high and upper bytes of the address bus to zero
04C30B             0009*  TMR_CTL:     equ 80h
04C30B             0010*  
04C30B             0011*  ; Timer Control Register Bit Definitions
04C30B             0012*  PRT_IRQ_0:    equ %00000000 ; The timer does not reach its end-of-count value.
04C30B             0013*                              ; This bit is reset to 0 every time the TMRx_CTL register is read.
04C30B             0014*  PRT_IRQ_1:    equ %10000000 ; The timer reaches its end-of-count value. If IRQ_EN is set to 1,
04C30B             0015*                              ; an interrupt signal is sent to the CPU. This bit remains 1 until
04C30B             0016*                              ; the TMRx_CTL register is read.
04C30B             0017*  
04C30B             0018*  IRQ_EN_0:     equ %00000000 ; Timer interrupt requests are disabled.
04C30B             0019*  IRQ_EN_1:     equ %01000000 ; Timer interrupt requests are enabled.
04C30B             0020*  
04C30B             0021*  PRT_MODE_0:   equ %00000000 ; The timer operates in SINGLE PASS mode. PRT_EN (bit 0) is reset to
04C30B             0022*                              ;  0, and counting stops when the end-of-count value is reached.
04C30B             0023*  PRT_MODE_1:   equ %00010000 ; The timer operates in CONTINUOUS mode. The timer reload value is
04C30B             0024*                              ; written to the counter when the end-of-count value is reached.
04C30B             0025*  
04C30B             0026*  ; CLK_DIV is a 2-bit mask that sets the timer input source clock divider
04C30B             0027*  CLK_DIV_256:  equ %00001100 ;
04C30B             0028*  CLK_DIV_64:   equ %00001000 ;
04C30B             0029*  CLK_DIV_16:   equ %00000100 ;
04C30B             0030*  CLK_DIV_4:    equ %00000000 ;
04C30B             0031*  
04C30B             0032*  RST_EN_0:     equ %00000000 ; The reload and restart function is disabled.
04C30B             0033*  RST_EN_1:     equ %00000010 ; The reload and restart function is enabled.
04C30B             0034*                              ; When a 1 is written to this bit, the values in the reload registers
04C30B             0035*                              ;  are loaded into the downcounter when the timer restarts. The
04C30B             0036*                              ; programmer must ensure that this bit is set to 1 each time
04C30B             0037*                              ; SINGLE-PASS mode is used.
04C30B             0038*  
04C30B             0039*  ; disable/enable the programmable reload timer
04C30B             0040*  PRT_EN_0:     equ %00000000 ;
04C30B             0041*  PRT_EN_1:     equ %00000001 ;
04C30B             0042*  
04C30B             0043*  ; Table 37. Timer Input Source Select Register
04C30B             0044*  ; Each of the 4 timers are allocated two bits of the 8-bit register
04C30B             0045*  ; in little-endian order, with TMR0 using bits 0 and 1, TMR1 using bits 2 and 3, etc.
04C30B             0046*  ;   00: System clock / CLK_DIV
04C30B             0047*  ;   01: RTC / CLK_DIV
04C30B             0048*  ;   NOTE: these are the values given in the manual, but it may be a typo
04C30B             0049*  ;   10: GPIO port B pin 1.
04C30B             0050*  ;   11: GPIO port B pin 1.
04C30B             0051*  TMR_ISS:   equ 92h ; register address
04C30B             0052*  
04C30B             0053*  ; Table 51. Real-Time Clock Control Register
04C30B             0054*  RTC_CTRL: equ EDh ; register address
04C30B             0055*  
04C30B             0056*  ; alarm interrupt disable/enable
04C30B             0057*  RTC_ALARM_0:    equ %00000000
04C30B             0058*  RTC_ALARM_1:    equ %10000000
04C30B             0059*  
04C30B             0060*  ; interrupt on alarm disable/enable
04C30B             0061*  RTC_INT_ENT_0:  equ %00000000
04C30B             0062*  RTC_INT_ENT_1:  equ %01000000
04C30B             0063*  
04C30B             0064*  RTC_BCD_EN_0:   equ %00000000   ; RTC count and alarm registers are binary
04C30B             0065*  RTC_BCD_EN_1:   equ %00100000   ; RTC count and alarm registers are BCD
04C30B             0066*  
04C30B             0067*  RTC_CLK_SEL_0:  equ %00000000   ; RTC clock source is crystal oscillator output (32768 Hz).
04C30B             0068*                                  ; On-chip 32768 Hz oscillator is enabled.
04C30B             0069*  RTC_CLK_SEL_1:  equ %00010000   ; RTC clock source is power line frequency input as set by FREQ_SEL.
04C30B             0070*                                  ; On-chip 32768 Hz oscillator is disabled.
04C30B             0071*  
04C30B             0072*  RTC_FREQ_SEL_0: equ %00000000   ; 60 Hz power line frequency.
04C30B             0073*  RTC_FREQ_SEL_1: equ %00001000   ; 50 Hz power line frequency.
04C30B             0074*  
04C30B             0075*  RTC_SLP_WAKE_0: equ %00000000   ; RTC does not generate a sleep-mode recovery reset.
04C30B             0076*  RTC_SLP_WAKE_1: equ %00000010   ; RTC generates a sleep-mode recovery reset.
04C30B             0077*  
04C30B             0078*  RTC_UNLOCK_0:   equ %00000000   ; RTC count registers are locked to prevent Write access.
04C30B             0079*                                  ; RTC counter is enabled.
04C30B             0080*  RTC_UNLOCK_1:   equ %00000001   ; RTC count registers are unlocked to allow Write access.
04C30B             0081*                                  ; RTC counter is disabled.
