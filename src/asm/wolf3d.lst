049D2E             0001*  ; ###### SPRITE TABLE FIELD INDICES ######
049D2E             0002*  sprite_id:              equ 00 ; 1 byte  - unique spriteId, zero-based
049D2E             0003*  sprite_obj:             equ 01 ; 1 byte  - type of sprite as defined in polys.asm, 255 is dead
049D2E             0004*  sprite_health:          equ 02 ; 1 byte  - health points, signed binary, zero or negative is dead
049D2E             0005*  sprite_behavior_index:  equ 03 ; 1 byte  - index of sprite's behavior subroutine in enemies.asm
049D2E             0006*  sprite_x:               equ 04 ; 1 byte  - map x position
049D2E             0007*  sprite_y:               equ 05 ; 1 byte  - map y position
049D2E             0008*  sprite_orientation:     equ 06 ; 1 byte  - orientation
049D2E             0009*  sprite_animation:       equ 07 ; 1 byte  - current animation index, zero-based
049D2E             0010*  sprite_animation_timer: equ 08 ; 1 byte  - when hits zero, draw next animation frame
049D2E             0011*  sprite_move_timer:      equ 09 ; 1 byte  - when zero, go to next move program, or step
049D2E             0012*  sprite_move_step:       equ 10 ; 1 byte  - stage in a move program sequence, varies
049D2E             0013*  sprite_points:          equ 11 ; 1 byte  - points awarded for killing this sprite type, BCD
049D2E             0014*  sprite_health_modifier:   equ 12 ; 1 byte  - health points deducted per successful attack on player, signed binary (positive gains health)
049D2E             0015*  sprite_unassigned:      equ 13 ; 3 bytes - unassigned can be used for custom properties
049D2E             0016*  sprite_record_size: equ 16 ; 16 bytes per sprite record
049D2E             0017*  
049D2E             0018*  
049D2E             0019*  ; ###### SPRITE TABLE VARIABLES ######
049D2E             0020*  ; maximum number of sprites
049D2E             0021*  table_max_records:      equ 64 ; at 16 bytes per record = 1024 bytes + 7 KiB for the map is an even 8 KiB
049D2E             0022*  table_total_bytes:      equ table_max_records*sprite_record_size
049D2E             0023*  
049D2E             0024*  ; #### THIS DEFINES THE SPACE ALLOCATED TO THE SPRITE TABLE IN EACH MAP FILE ####
049D2E             0025*  sprite_table_base:       equ 0xB7FC00
049D2E             0026*  sprite_table_limit:      equ sprite_table_base + table_total_bytes + 1 ; in case we ever need to know where it ends
049D2E             0027*  
049D2E             0028*  ; pointer to top address of current record, initialized to sprite_table_base
049D2E 00 FC B7    0029*  sprite_table_pointer: dl sprite_table_base
049D31             0030*  ; how many active sprites
049D31 00          0031*  table_active_sprites: db 0x00
049D32             0032*  ; flag indicating collision with screen edge
049D32             0033*  ; uses orientation codes to specify which edge(s)
049D32 00          0034*  sprite_screen_edge: db #00
049D33             0035*  ; next sprite id to use
049D33 00          0036*  sprite_next_id: db 0
049D34             0037*  
049D34             0038*  ; ######### COLLISION SPRITE PARAMETERS ##########
049D34             0039*  ; integer coordinates are all that are needed for collision calculations
049D34 00          0040*  collision_x: db 0x00
049D35 00          0041*  collision_y: db 0x00
049D36 00          0042*  collision_dim_x: db 0x00
049D37 00          0043*  collision_dim_y: db 0x00
049D38             0044*  
049D38             0045*  ; scratch variables
049D38 00          0046*  x: db 0x00 ; 8-bit signed integer
049D39 00          0047*  y: db 0x00 ; 8-bit signed integer
049D3A 00 00 00    0048*  x0: dl 0x000000 ; 16.8 signed fixed place
049D3D 00 00 00    0049*  y0: dl 0x000000 ; 16.8 signed fixed place
049D40 00 00 00    0050*  incx1: dl 0x000000 ; 16.8 signed fixed place
049D43 00 00 00    0051*  incy1: dl 0x000000 ; 16.8 signed fixed place
049D46 00 00 00    0052*  incx2: dl 0x000000 ; 16.8 signed fixed place
049D49 00 00 00    0053*  incy2: dl 0x000000 ; 16.8 signed fixed place
049D4C             0054*  
049D4C             0055*  ; sprite_heading: dl 0x000000 ; signed fixed 16.8
049D4C 00 00 00    0056*  radius: dl 0x000000 ; signed fixed 16.8 (but should always be positive)
049D4F 00 00 00    0057*  sin_sprite_heading: dl 0x000000 ; signed fixed 16.8
049D52 00 00 00    0058*  cos_sprite_heading: dl 0x000000 ; signed fixed 16.8
049D55             0059*  
049D55             0060*  ; gets the next available sprite id
049D55             0061*  ; inputs; none
049D55             0062*  ; returns: if new sprite available, a = sprite id,
049D55             0063*  ;      ix pointing to new sprite vars, carry set
049D55             0064*  ;      otherwise, a = 0, carry flag reset, ix pointing to highest sprite vars
049D55             0065*  ; destroys: a,b,hl,ix
049D55             0066*  ; affects: bumps table_active_sprites by one
049D55             0067*  table_get_next_id:
049D55 DD 21 00 FC 0068*      ld ix,sprite_table_base
       B7          
049D5A 11 10 00 00 0069*      ld de,sprite_record_size
049D5E 06 40       0070*      ld b,table_max_records
049D60             0071*  @loop:
049D60 DD 7E 01    0072*      ld a,(ix+sprite_obj)
049D63 A7          0073*      and a
049D64 28 06       0074*      jr z,@found
049D66 DD 19       0075*      add ix,de
049D68 10 F6       0076*      djnz @loop
049D6A             0077*  @notfound:
049D6A AF          0078*      xor a ; a = 0 and reset carry flag indicating that we didn't find a free sprite
049D6B C9          0079*      ret
049D6C             0080*  @found:
049D6C             0081*  ; bump number of active sprites
049D6C 21 31 9D 04 0082*      ld hl,table_active_sprites
049D70 34          0083*      inc (hl)
049D71             0084*  ; return sprite id
049D71 3E 40       0085*      ld a,table_max_records
049D73 90          0086*      sub b
049D74 32 33 9D 04 0087*      ld (sprite_next_id),a
049D78 37          0088*      scf ; sets carry flag indicating we found a free sprite
049D79 C9          0089*      ret ; done
049D7A             0090*  
049D7A             0091*  ; deactivate the sprite with the given id
049D7A             0092*  ; inputs: a = sprite id
049D7A             0093*  ; outputs: nothing
049D7A             0094*  ; destroys: a,ix,de
049D7A             0095*  ; affects: decrements table_active_sprites by one
049D7A             0096*  table_deactivate_sprite:
049D7A F5          0097*      push af ; save sprite id bc we need it later
049D7B CD 60 A8 04 0098*      call vdu_sprite_select
049D7F CD E4 A8 04 0099*      call vdu_sprite_hide
049D83 F1          0100*      pop af ; restore sprite id
049D84 11 00 00 00 0101*      ld de,0 ; clear deu
049D88 57          0102*      ld d,a
049D89 1E 10       0103*      ld e,sprite_record_size
049D8B ED 5C       0104*      mlt de
049D8D DD 21 00 FC 0105*      ld ix,sprite_table_base
       B7          
049D92 DD 19       0106*      add ix,de
049D94 AF          0107*      xor a
049D95 DD 77 01    0108*      ld (ix+sprite_obj),a
049D98 DD 21 31 9D 0109*      ld ix,table_active_sprites
       04          
049D9D DD 35 00    0110*      dec (ix)
049DA0 C9          0111*      ret
049DA1             0112*  
049DA1             0113*  ; sets iy and sprite_table_pointer to the sprite record with the given id
049DA1             0114*  ; inputs: a = sprite id
049DA1             0115*  ; outputs: iy = sprite_table_pointer to the sprite record with the given id
049DA1             0116*  ; destroys: bc
049DA1             0117*  sprite_set_pointer:
049DA1 47          0118*      ld b,a
049DA2 0E 10       0119*      ld c,sprite_record_size
049DA4 ED 4C       0120*      mlt bc
049DA6 FD 21 00 FC 0121*      ld iy,sprite_table_base
       B7          
049DAB FD 09       0122*      add iy,bc
049DAD FD 22 2E 9D 0123*      ld (sprite_table_pointer),iy
       04          
049DB2 C9          0124*      ret
049DB3             0125*  
049DB3             0126*  ; set the active sprite record to no sprite and remove it from the map cell it was in
049DB3             0127*  ; inputs: iy pointed at sprite record to clear
049DB3             0128*  sprite_kill:
049DB3             0129*  ; set sprite table record to no sprite
049DB3 21 FF FF FF 0130*      ld hl,0xFFFFFF ; a string of -1s
049DB7 FD 2F 00    0131*      ld (iy),hl ; populates sprite_id, sprite_obj and sprite_heatlth with -1, all indicating that it is quite dead
049DBA             0132*  
049DBA             0133*  ; set map cell to no sprite and normal floor
049DBA 21 01 FF 1D 0134*      ld hl,0x1DFF01 ; normal floor TODO: we should set these values dyanmically based on the defs in tiles.txt at some point
049DBE DD 2F 00    0135*      ld (ix),hl
049DC1 3E FF       0136*      ld a,0xFF ; no sprite
049DC3 DD 77 03    0137*      ld (ix+map_sprite_id),a ; now sprite is truly dead
049DC6 C9          0138*      ret
