04A654             0031   	include "src/asm/vdu.asm"
04A654             0001*  ; The following is a high-level list of the VDU sequences that are supported:
04A654             0002*  ; VDU 0: Null (no operation)
04A654             0003*  ; VDU 1: Send next character to printer (if printer is enabled) 
04A654             0004*  ; VDU 2: Enable printer 
04A654             0005*  ; VDU 3: Disable printer 
04A654             0006*  ; VDU 4: Write text at text cursor
04A654             0007*  ; VDU 5: Write text at graphics cursor
04A654             0008*  ; VDU 6: Enable screen (opposite of VDU 21) 
04A654             0009*  ; VDU 7: Make a short beep (BEL)
04A654             0010*  ; VDU 8: Move cursor back one character
04A654             0011*  ; VDU 9: Move cursor forward one character
04A654             0012*  ; VDU 10: Move cursor down one line
04A654             0013*  ; VDU 11: Move cursor up one line
04A654             0014*  ; VDU 12: Clear text area (CLS)
04A654             0015*  ; VDU 13: Carriage return
04A654             0016*  ; VDU 14: Page mode On *
04A654             0017*  ; VDU 15: Page mode Off *
04A654             0018*  ; VDU 16: Clear graphics area (CLG)
04A654             0019*  ; VDU 17, colour: Define text colour (COLOUR)
04A654             0020*  ; VDU 18, mode, colour: Define graphics colour (GCOL mode, colour)
04A654             0021*  ; VDU 19, l, p, r, g, b: Define logical colour (COLOUR l, p / COLOUR l, r, g, b)
04A654             0022*  ; VDU 20: Reset palette and text/graphics colours and drawing modes 
04A654             0023*  ; VDU 21: Disable screen (turns of VDU command processing, except for VDU 1 and VDU 6) 
04A654             0024*  ; VDU 22, n: Select screen mode (MODE n)
04A654             0025*  ; VDU 23, n: Re-program display character / System Commands
04A654             0026*  ; VDU 24, left; bottom; right; top;: Set graphics viewport **
04A654             0027*  ; VDU 25, mode, x; y;: PLOT command
04A654             0028*  ; VDU 26: Reset graphics and text viewports **
04A654             0029*  ; VDU 27, char: Output character to screen 
04A654             0030*  ; VDU 28, left, bottom, right, top: Set text viewport **
04A654             0031*  ; VDU 29, x; y;: Set graphics origin
04A654             0032*  ; VDU 30: Home cursor
04A654             0033*  ; VDU 31, x, y: Move text cursor to x, y text position (TAB(x, y))
04A654             0034*  ; VDU 127: Backspace
04A654             0035*  
04A654             0036*  ; VDU 0: Null (no operation)
04A654             0037*  ;     On encountering a VDU 0 command, the VDP will do nothing.
04A654             0038*  ;     This may be useful for padding out a VDU command sequence,
04A654             0039*  ;     or for inserting a placeholder for a command that will be added later.
04A654             0040*  ; inputs: none
04A654             0041*  ; outputs: an empty byte somewhere in VDU
04A654             0042*  ; destroys: a
04A654             0043*  vdu_null:
04A654 AF          0044*      xor a
04A655 5B D7       0045*  	rst.lil $10
04A657 C9          0046*  	ret
04A658             0047*  
04A658             0048*  ; VDU 1: Send next character to printer (if printer is enabled) 
04A658             0049*  ;     Ensures that the next character received by the VDP is sent through to
04A658             0050*  ;     the printer, and not to the screen. This is useful for sending control
04A658             0051*  ;     codes to the printer, or for sending data to the printer that is not
04A658             0052*  ;     intended to be displayed on the screen. It allows characters that would
04A658             0053*  ;     not otherwise normally be sent through to the printer to be sent.
04A658             0054*  ;     If the printer has not been enabled then this command will just discard
04A658             0055*  ;     the next byte sent to the VDP.
04A658             0056*  ; inputs: a is the ascii code of the character to send
04A658             0057*  ; prerequisites: "printer" must first be activated with VDU 2 (see below)
04A658             0058*  ; outputs: a character on the serial terminal connected to the USB port
04A658             0059*  ;           and the same character on the screen at the current text cursor location
04A658             0060*  ; QUESTION: does it also advance the text cursor?
04A658             0061*  ; destroys: hl, bc
04A658             0062*  vdu_char_to_printer:
04A658 32 68 A6 04 0063*  	ld (@arg),a
04A65C 21 67 A6 04 0064*  	ld hl,@cmd
04A660 01 02 00 00 0065*  	ld bc,@end-@cmd
04A664 5B DF       0066*  	rst.lil $18
04A666 C9          0067*  	ret
04A667 01          0068*  @cmd: db 1
04A668 00          0069*  @arg: db 0
04A669             0070*  @end:
04A669             0071*  
04A669             0072*  ; VDU 2: Enable printer 
04A669             0073*  ;     Enables the printer.
04A669             0074*  ;     In the context of the Agon platform, the printer is a serial
04A669             0075*  ;     terminal that is connected to the VDPs USB port. Typically
04A669             0076*  ;     this port is used for power, but it can also be used to send and
04A669             0077*  ;     receive data to and from the VDP.
04A669             0078*  ;     When the printer is enabled, the VDP will send characters it receives
04A669             0079*  ;     to the printer as well as to the screen. It will additionally send
04A669             0080*  ;     through control codes 8-13. To send other control codes to the printer,
04A669             0081*  ;     use the VDU 1 command.
04A669             0082*  ;     The VDP will not send through other control codes to the printer,
04A669             0083*  ;     and will will not send through data it receives as part of other commands.
04A669             0084*  vdu_enable_printer:
04A669 3E 02       0085*      ld a,2
04A66B 5B D7       0086*  	rst.lil $10
04A66D C9          0087*  	ret
04A66E             0088*  
04A66E             0089*  ; VDU 3: Disable printer 
04A66E             0090*  ; inputs: none
04A66E             0091*  ; outputs: a USB port bereft of communication with the VDP
04A66E             0092*  ; destroys: a
04A66E             0093*  vdu_disable_printer:
04A66E 3E 03       0094*      ld a,3
04A670 5B D7       0095*  	rst.lil $10
04A672 C9          0096*  	ret
04A673             0097*  
04A673             0098*  ; VDU 4: Write text at text cursor
04A673             0099*  ;     This causes text to be written at the current text cursor position.
04A673             0100*  ;     This is the default mode for text display.
04A673             0101*  ;     Text is written using the current text foreground and background colours.
04A673             0102*  ; inputs: a is the character to write to the screen
04A673             0103*  ; prerequisites: the text cursor at the intended position on screen
04A673             0104*  ; outputs: prints the character and moves text cursor right one position
04A673             0105*  ; destroys: a, hl, bc
04A673             0106*  vdu_char_to_text_cursor:
04A673 32 83 A6 04 0107*  	ld (@arg),a
04A677 21 82 A6 04 0108*  	ld hl,@cmd
04A67B 01 02 00 00 0109*  	ld bc,@end-@cmd
04A67F 5B DF       0110*  	rst.lil $18
04A681 C9          0111*  	ret
04A682 04          0112*  @cmd: db 4
04A683 00          0113*  @arg: db 0
04A684             0114*  @end:
04A684             0115*  
04A684             0116*  ; VDU 5: Write text at graphics cursor
04A684             0117*  ;     This causes text to be written at the current graphics cursor position.
04A684             0118*  ;     Using this, characters may be positioned at any graphics coordinate within
04A684             0119*  ;     the graphics viewport. This is useful for positioning text over graphics,
04A684             0120*  ;     or for positioning text at a specific location on the screen.
04A684             0121*  ;     Characters are plotted using the current graphics foreground colour,
04A684             0122*  ;     using the current graphics foreground plotting mode (see VDU 18).
04A684             0123*  ;     The character background is transparent, and will not overwrite any
04A684             0124*  ;     graphics that are already present at the characters location.
04A684             0125*  ;     The exception to this is VDU 27, the delete character, which backspaces
04A684             0126*  ;     and deletes as per its usual behaviour, but will erase using the current
04A684             0127*  ;     graphics background colour.
04A684             0128*  ; inputs: a is the character to write to the screen
04A684             0129*  ; prerequisites: the graphics cursor at the intended position on screen
04A684             0130*  ; outputs: see the name of the function
04A684             0131*  ; destroys: a, hl, bc
04A684             0132*  vdu_char_to_gfx_cursor:
04A684 32 94 A6 04 0133*  	ld (@arg),a
04A688 21 93 A6 04 0134*  	ld hl,@cmd
04A68C 01 02 00 00 0135*  	ld bc,@end-@cmd
04A690 5B DF       0136*  	rst.lil $18
04A692 C9          0137*  	ret
04A693 05          0138*  @cmd: db 5
04A694 00          0139*  @arg: db 0
04A695             0140*  @end:
04A695             0141*  
04A695             0142*  ; VDU 6: Enable screen (opposite of VDU 21) 
04A695             0143*  ;     This enables the screen, and re-enables VDU command processing,
04A695             0144*  ;     reversing the effect of VDU 21.
04A695             0145*  ; inputs: none
