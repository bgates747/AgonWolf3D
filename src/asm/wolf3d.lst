04AB45 01 06 00 00 0313*  	ld bc,@end-@cmd
04AB49 5B DF       0314*  	rst.lil $18
04AB4B C9          0315*  	ret
04AB4C 13          0316*  @cmd: db 19
04AB4D 00          0317*  @logical: db 0
04AB4E 00          0318*  @physical: db 0
