04A5E7             0189*  vdu_cursor_down:
04A5E7 3E 0A       0190*      ld a,10
04A5E9 5B D7       0191*  	rst.lil $10
04A5EB C9          0192*  	ret
04A5EC             0193*  
04A5EC             0194*  ; VDU 11: Move cursor up one line
04A5EC             0195*  vdu_cursor_up:
04A5EC 3E 0B       0196*      ld a,11
