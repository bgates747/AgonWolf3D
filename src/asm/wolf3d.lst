04A21A             0023   	include "src/asm/sprites.asm"
04A21A             0001*  ; ###### SPRITE TABLE FIELD INDICES ######
04A21A             0002*  sprite_id:              equ 00 ; 1 byte  - unique spriteId, zero-based
04A21A             0003*  sprite_obj:             equ 01 ; 1 byte  - type of sprite as defined in polys.asm, 255 is dead
04A21A             0004*  sprite_health:          equ 02 ; 1 byte  - health points, signed binary, zero or negative is dead
04A21A             0005*  sprite_triggers_mask:   equ 03 ; 1 byte  - bitmask for tracking which sprite behaviors have been triggered
04A21A             0006*  sprite_x:               equ 04 ; 1 byte  - map x position
04A21A             0007*  sprite_y:               equ 05 ; 1 byte  - map y position
04A21A             0008*  sprite_orientation:     equ 06 ; 1 byte  - orientation
04A21A             0009*  sprite_animation:       equ 07 ; 1 byte  - current animation index, zero-based
04A21A             0010*  sprite_anim_timer:      equ 08 ; 1 byte  - when hits zero, draw next animation frame
04A21A             0011*  sprite_move_timer:      equ 09 ; 1 byte  - when zero, go to next move program, or step
04A21A             0012*  sprite_move_step:       equ 10 ; 1 byte  - stage in a move program sequence, varies
04A21A             0013*  sprite_points:          equ 11 ; 1 byte  - points awarded for killing this sprite type, binary
04A21A             0014*  sprite_health_modifier: equ 12 ; 1 byte  - health points deducted per successful attack on player, signed binary (positive gains health)
04A21A             0015*  sprite_unassigned:      equ 13 ; 3 bytes - unassigned can be used for custom properties
04A21A             0016*  sprite_record_size:     equ 16 ; 16 bytes per sprite record
04A21A             0017*  
04A21A             0018*  ; sprite_triggers_mask defs
04A21A             0019*  sprite_trigger_see:     equ %00000001 ; player has been seen
04A21A             0020*  sprite_trigger_use:     equ %00000010 ; player has used the sprite
