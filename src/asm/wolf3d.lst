04C65E             0014*  PRT_IRQ_1:    equ %10000000 ; The timer reaches its end-of-count value. If IRQ_EN is set to 1,
04C65E             0015*                              ; an interrupt signal is sent to the CPU. This bit remains 1 until
04C65E             0016*                              ; the TMRx_CTL register is read.
04C65E             0017*  
04C65E             0018*  IRQ_EN_0:     equ %00000000 ; Timer interrupt requests are disabled.
04C65E             0019*  IRQ_EN_1:     equ %01000000 ; Timer interrupt requests are enabled.
04C65E             0020*  
04C65E             0021*  PRT_MODE_0:   equ %00000000 ; The timer operates in SINGLE PASS mode. PRT_EN (bit 0) is reset to
04C65E             0022*                              ;  0,and counting stops when the end-of-count value is reached.
04C65E             0023*  PRT_MODE_1:   equ %00010000 ; The timer operates in CONTINUOUS mode. The timer reload value is
04C65E             0024*                              ; written to the counter when the end-of-count value is reached.
04C65E             0025*  
