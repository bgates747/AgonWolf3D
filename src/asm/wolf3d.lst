04AA8F             0098*  ; VDU 4: Write text at text cursor
04AA8F             0099*  ;     This causes text to be written at the current text cursor position.
04AA8F             0100*  ;     This is the default mode for text display.
04AA8F             0101*  ;     Text is written using the current text foreground and background colours.
04AA8F             0102*  ; inputs: a is the character to write to the screen
04AA8F             0103*  ; prerequisites: the text cursor at the intended position on screen
04AA8F             0104*  ; outputs: prints the character and moves text cursor right one position
04AA8F             0105*  ; destroys: a, hl, bc
04AA8F             0106*  vdu_char_to_text_cursor:
04AA8F 32 9F AA 04 0107*  	ld (@arg),a
04AA93 21 9E AA 04 0108*  	ld hl,@cmd
04AA97 01 02 00 00 0109*  	ld bc,@end-@cmd
04AA9B 5B DF       0110*  	rst.lil $18
04AA9D C9          0111*  	ret
04AA9E 04          0112*  @cmd: db 4
04AA9F 00          0113*  @arg: db 0
04AAA0             0114*  @end:
