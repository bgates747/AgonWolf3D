04650D                   0002 ; map tables addresses
04650D                   0003 cell_status: 		equ 0xB7E000 ; base of on-chip high speed SRAM
04650D                   0004 cell_views: 		equ 0xB7E400 ; cell_status + 256*4
04650D                   0005 
04650D                   0006 ; map type/status flags
04650D                   0007 cell_is_door:     	EQU %10000000  ; Bit 7: door flag
04650D                   0008 cell_is_wall:     	EQU %01000000  ; Bit 6: wall flag
04650D                   0009 cell_is_trigger:  	EQU %00100000  ; Bit 5: trigger flag
04650D                   0010 cell_is_blocking: 	EQU %00010000  ; Bit 4: blocking flag
04650D                   0011 cell_is_start:		EQU %00001000  ; Bit 3: start flag
04650D                   0012 
04650D                   0013 ; HOW THE MAP TABLE IS LAID OUT
04650D                   0014 ; (obj_id render_obj type/status mask), render routine address
04650D                   0015 ; cell_000: dl 0x110000,rend_000; 0x00 : 00,00 : 0x00,0x00
04650D                   0016 ;
04650D                   0017 ; map table field offset constants
04650D                   0018 ; (mind the little-endianess)
04650D                   0019 map_type_status: 	equ 0
04650D                   0020 map_img_idx: 		equ 1
04650D                   0021 map_obj_id: 		equ 2
04650D                   0022 ; map_render: 		equ 3 ; deprecated
04650D                   0023 map_sprite_id: 		equ 3
04650D                   0024 map_record_size: 	equ 4 ; bytes per cell_status record
04650D                   0025 
04650D                   0026 ; gets cell info from a directional displacement from a given map location
04650D                   0027 ; inputs: ; d = distance ; e = direction, b,c = y,x
04650D                   0028 ; returns: ix = pointer to cell_status lut; a = obj_id; hl = address of cell base render routine
04650D                   0029 ; calls: get_dx_dy, get_cell_from_coords
04650D                   0030 get_neighbor:
04650D                   0031 ; modulo 4 on orientation
04650D 7B                0032     ld a,e
04650E E6 03             0033     and 0x03
046510 5F                0034     ld e,a
046511 CD 40 65 04       0035 	call get_dx_dy ; d,e = dy,dx
046515                   0036 ; add add b,c to the deltas in d,e
046515 79                0037 	ld a,c
046516 83                0038 	add a,e
046517 5F                0039 	ld e,a
046518 78                0040 	ld a,b
046519 82                0041 	add a,d
04651A 57                0042 	ld d,a
04651B                   0043 ; fall through to get_cell_from_coords
04651B                   0044 
04651B                   0045 ; gets cell info from a given x,y map coordinate
04651B                   0046 ; inputs: ; d,e = map_y,map_x
04651B                   0047 ; returns: ix = pointer to cell_status lut; a = obj_id, bc = cell_id
04651B                   0048 get_cell_from_coords:
04651B                   0049 ; modulo 16 on input coords
04651B 7B                0050 	ld a,e
04651C E6 0F             0051 	and 0x0F
04651E 5F                0052 	ld e,a
04651F 7A                0053 	ld a,d
046520 E6 0F             0054 	and 0x0F
046522 57                0055 	ld d,a
046523                   0056 ; get cell_id from x,y
046523 42                0057 	ld b,d ; y
046524 0E 10             0058 	ld c,16 ; number of cells in a column
046526 ED 4C             0059 	mlt bc ; bc = cell_id of y,0
046528 21 00 00 00       0060 	ld hl,0 ; make sure uhl is zero
04652C 6B                0061 	ld l,e ; x
04652D 09                0062 	add hl,bc ; hl = cell_id of x,y
04652E E5                0063 	push hl ; so we can return cell_id
04652F 4D                0064 	ld c,l ; c = cell_id
046530                   0065 ; get address of cell record in cell_status table
046530 06 04             0066 	ld b,map_record_size ; b = bytes per record
046532 ED 4C             0067 	mlt bc ; bc = offset to cell record
046534 DD 21 00 E0 B7    0068 	ld ix,cell_status ; base address of lookup table
046539 DD 09             0069 	add ix,bc ; ix = address of cell record
04653B DD 7E 02          0070 	ld a,(ix+map_obj_id) ; a = obj_id
04653E C1                0071 	pop bc ; bc = cell_id
04653F C9                0072 	ret
046540                   0073 
046540                   0074 ; gets dx,dy from orientation and distance
046540                   0075 ; d = distance ; e = orientation
046540                   0076 ; returns: d,e = dy,dx
046540                   0077 get_dx_dy:
046540                   0078 ; modulo 4 on orientation
046540 7B                0079 	ld a,e
046541 E6 03             0080 	and 0x03
046543 FE 00             0081 	cp 0
046545 28 11             0082 	jr z,@north
046547 FE 01             0083 	cp 1
046549 28 14             0084 	jr z,@east
04654B FE 02             0085 	cp 2
04654D 28 14             0086 	jr z,@south
04654F FE 03             0087 	cp 3
046551 28 13             0088 	jr z,@west
046553                   0089 ; if none of those, return zeroes
046553 11 00 00 00       0090 	ld de,0
046557 C9                0091 	ret
046558                   0092 @north:
046558                   0093 ; x = 0, y = -d
046558 1E 00             0094 	ld e,0
