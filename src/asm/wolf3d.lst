04C1BB             0001*  ; Table 32. Timer Control Registers
04C1BB             0002*  ; this constant is the base address of the timer control registers
04C1BB             0003*  ; each timer takes three bytes:
04C1BB             0004*  ;   0: control register
04C1BB             0005*  ;   1: low byte of timer reset value
04C1BB             0006*  ;   2: high byte of timer reset value
04C1BB             0007*  ; note that the value is only 8-bits,so we use in0/out0 i/o calls,
04C1BB             0008*  ; which correctly force the high and upper bytes of the address bus to zero
04C1BB             0009*  TMR_CTL:     equ 80h
04C1BB             0010*  
04C1BB             0011*  ; Timer Control Register Bit Definitions
04C1BB             0012*  PRT_IRQ_0:    equ %00000000 ; The timer does not reach its end-of-count value.
04C1BB             0013*                              ; This bit is reset to 0 every time the TMRx_CTL register is read.
04C1BB             0014*  PRT_IRQ_1:    equ %10000000 ; The timer reaches its end-of-count value. If IRQ_EN is set to 1,
04C1BB             0015*                              ; an interrupt signal is sent to the CPU. This bit remains 1 until
04C1BB             0016*                              ; the TMRx_CTL register is read.
04C1BB             0017*  
04C1BB             0018*  IRQ_EN_0:     equ %00000000 ; Timer interrupt requests are disabled.
04C1BB             0019*  IRQ_EN_1:     equ %01000000 ; Timer interrupt requests are enabled.
04C1BB             0020*  
04C1BB             0021*  PRT_MODE_0:   equ %00000000 ; The timer operates in SINGLE PASS mode. PRT_EN (bit 0) is reset to
04C1BB             0022*                              ;  0,and counting stops when the end-of-count value is reached.
04C1BB             0023*  PRT_MODE_1:   equ %00010000 ; The timer operates in CONTINUOUS mode. The timer reload value is
04C1BB             0024*                              ; written to the counter when the end-of-count value is reached.
04C1BB             0025*  
04C1BB             0026*  ; CLK_DIV is a 2-bit mask that sets the timer input source clock divider
04C1BB             0027*  CLK_DIV_256:  equ %00001100 ;
04C1BB             0028*  CLK_DIV_64:   equ %00001000 ;
04C1BB             0029*  CLK_DIV_16:   equ %00000100 ;
04C1BB             0030*  CLK_DIV_4:    equ %00000000 ;
04C1BB             0031*  
04C1BB             0032*  RST_EN_0:     equ %00000000 ; The reload and restart function is disabled.
04C1BB             0033*  RST_EN_1:     equ %00000010 ; The reload and restart function is enabled.
04C1BB             0034*                              ; When a 1 is written to this bit,the values in the reload registers
04C1BB             0035*                              ;  are loaded into the downcounter when the timer restarts. The
04C1BB             0036*                              ; programmer must ensure that this bit is set to 1 each time
04C1BB             0037*                              ; SINGLE-PASS mode is used.
04C1BB             0038*  
04C1BB             0039*  ; disable/enable the programmable reload timer
04C1BB             0040*  PRT_EN_0:     equ %00000000 ;
04C1BB             0041*  PRT_EN_1:     equ %00000001 ;
04C1BB             0042*  
04C1BB             0043*  ; Table 37. Timer Input Source Select Register
04C1BB             0044*  ; Each of the 4 timers are allocated two bits of the 8-bit register
04C1BB             0045*  ; in little-endian order,with TMR0 using bits 0 and 1,TMR1 using bits 2 and 3,etc.
04C1BB             0046*  ;   00: System clock / CLK_DIV
04C1BB             0047*  ;   01: RTC / CLK_DIV
04C1BB             0048*  ;   NOTE: these are the values given in the manual,but it may be a typo
04C1BB             0049*  ;   10: GPIO port B pin 1.
04C1BB             0050*  ;   11: GPIO port B pin 1.
04C1BB             0051*  TMR_ISS:   equ 92h ; register address
04C1BB             0052*  
04C1BB             0053*  ; Table 51. Real-Time Clock Control Register
04C1BB             0054*  RTC_CTRL: equ EDh ; register address
04C1BB             0055*  
04C1BB             0056*  ; alarm interrupt disable/enable
04C1BB             0057*  RTC_ALARM_0:    equ %00000000
04C1BB             0058*  RTC_ALARM_1:    equ %10000000
