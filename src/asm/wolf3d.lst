04656D                   0116 ; gets the direction from a dy,dx pair
04656D                   0117 ; basically atan2(dy,dx) but for only the 4 cardinal directions
04656D                   0118 ; inputs: ; d = dy ; e = dx
04656D                   0119 ; returns: a = orientation
04656D                   0120 get_dir_from_dy_dx:
04656D AF                0121 	xor a
04656E 93                0122 	sub e
04656F 28 06             0123 	jr z,@not_x
046571 3E 01             0124 	ld a,1
046573 F8                0125 	ret m
046574 3E 03             0126 	ld a,3
046576 C9                0127 	ret
046577                   0128 @not_x:
046577 AF                0129 	xor a
046578 92                0130 	sub d
046579 C8                0131 	ret z
04657A 3E 02             0132 	ld a,2
04657C F8                0133 	ret m
04657D AF                0134 	xor a
04657E C9                0135 	ret
