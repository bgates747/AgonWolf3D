04AC5B             0337*  ; VDU 22, n: Select screen mode (MODE n)
04AC5B             0338*  ; Inputs: a, screen mode (8-bit unsigned integer), in the following list:
04AC5B             0339*  ; https://agonconsole8.github.io/agon-docs/VDP---Screen-Modes.html
04AC5B             0340*  ; Screen modes
04AC5B             0341*  ; Modes over 128 are double-buffered
04AC5B             0342*  ; From Version 1.04 or greater
04AC5B             0343*  ; Mode 	Horz 	Vert 	Cols 	Refresh
04AC5B             0344*  ; 0 	640 	480 	16 	    60hz
04AC5B             0345*  ; * 1 	640 	480 	4 	    60hz
04AC5B             0346*  ; 2 	640 	480 	2 	    60hz
04AC5B             0347*  ; 3 	640 	240 	64 	    60hz
04AC5B             0348*  ; 4 	640 	240 	16 	    60hz
04AC5B             0349*  ; 5 	640 	240 	4 	    60hz
04AC5B             0350*  ; 6 	640 	240 	2 	    60hz
04AC5B             0351*  ; ** 7 	n/a 	n/a 	16 	    60hz
04AC5B             0352*  ; 8 	320 	240 	64 	    60hz
04AC5B             0353*  ; 9 	320 	240 	16 	    60hz
04AC5B             0354*  ; 10 	320 	240 	4 	    60hz
04AC5B             0355*  ; 11 	320 	240 	2 	    60hz
04AC5B             0356*  ; 12 	320 	200 	64 	    70hz
04AC5B             0357*  ; 13 	320 	200 	16 	    70hz
04AC5B             0358*  ; 14 	320 	200 	4 	    70hz
04AC5B             0359*  ; 15 	320 	200 	2 	    70hz
04AC5B             0360*  ; 16 	800 	600 	4 	    60hz
04AC5B             0361*  ; 17 	800 	600 	2 	    60hz
04AC5B             0362*  ; 18 	1024 	768 	2 	    60hz
04AC5B             0363*  ; 129 	640 	480 	4 	    60hz
04AC5B             0364*  ; 130 	640 	480 	2 	    60hz
04AC5B             0365*  ; 132 	640 	240 	16 	    60hz
04AC5B             0366*  ; 133 	640 	240 	4 	    60hz
04AC5B             0367*  ; 134 	640 	240 	2 	    60hz
04AC5B             0368*  ; 136 	320 	240 	64 	    60hz
04AC5B             0369*  ; 137 	320 	240 	16 	    60hz
04AC5B             0370*  ; 138 	320 	240 	4 	    60hz
04AC5B             0371*  ; 139 	320 	240 	2 	    60hz
04AC5B             0372*  ; 140 	320 	200 	64 	    70hz
