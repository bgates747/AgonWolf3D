04A60A             0023   	include "src/asm/sprites.asm"
04A60A             0001*  ; ###### SPRITE TABLE FIELD INDICES ######
04A60A             0002*  sprite_id:              equ 00 ; 1 byte  - unique spriteId, zero-based
04A60A             0003*  sprite_obj:             equ 01 ; 1 byte  - type of sprite as defined in polys.asm, 255 is dead
04A60A             0004*  sprite_health:          equ 02 ; 1 byte  - health points, signed binary, zero or negative is dead
04A60A             0005*  sprite_triggers_mask:   equ 03 ; 1 byte  - bitmask for tracking which sprite behaviors have been triggered
04A60A             0006*  sprite_x:               equ 04 ; 1 byte  - map x position
04A60A             0007*  sprite_y:               equ 05 ; 1 byte  - map y position
04A60A             0008*  sprite_orientation:     equ 06 ; 1 byte  - orientation
04A60A             0009*  sprite_animation:       equ 07 ; 1 byte  - current animation index, zero-based
04A60A             0010*  sprite_anim_timer:      equ 08 ; 1 byte  - when hits zero, draw next animation frame
04A60A             0011*  sprite_move_timer:      equ 09 ; 1 byte  - when zero, go to next move program, or step
04A60A             0012*  sprite_move_step:       equ 10 ; 1 byte  - stage in a move program sequence, varies
04A60A             0013*  sprite_points:          equ 11 ; 1 byte  - points awarded for killing this sprite type, binary
04A60A             0014*  sprite_health_modifier: equ 12 ; 1 byte  - health points deducted per successful attack on player, signed binary (positive gains health)
04A60A             0015*  sprite_unassigned:      equ 13 ; 3 bytes - unassigned can be used for custom properties
04A60A             0016*  sprite_record_size:     equ 16 ; 16 bytes per sprite record
04A60A             0017*  
04A60A             0018*  ; sprite_triggers_mask defs
04A60A             0019*  sprite_trigger_see:     equ %00000001 ; player has been seen
04A60A             0020*  sprite_trigger_use:     equ %00000010 ; player has used the sprite
04A60A             0021*  sprite_trigger_hurt:    equ %00000100 ; player has hurt the sprite
04A60A             0022*  sprite_trigger_kill:    equ %00001000 ; player has killed the sprite
04A60A             0023*  sprite_trigger_move:    equ %00010000 ; sprite has moved
04A60A             0024*  sprite_trigger_shoot:   equ %00100000 ; sprite has shot
04A60A             0025*  
04A60A             0026*  ; obj_ids for selected sprites
04A60A             0027*  ; these are the sprite_obj values for the selected sprites
04A60A             0028*  OBJ_ID_DEAD_GUARD:      equ 56
04A60A             0029*  OBJ_ID_EXPLOSION:       equ 60
04A60A             0030*  
04A60A             0031*  ; ###### SPRITE TABLE VARIABLES ######
04A60A             0032*  ; maximum number of sprites
04A60A             0033*  table_max_records:      equ 64 ; at 16 bytes per record = 1024 bytes + 7 KiB for the map is an even 8 KiB
04A60A             0034*  table_total_bytes:      equ table_max_records*sprite_record_size
04A60A             0035*  
04A60A             0036*  ; #### THIS DEFINES THE SPACE ALLOCATED TO THE SPRITE TABLE IN EACH MAP FILE ####
04A60A             0037*  sprite_table_base:       equ 0xB7FC00
04A60A             0038*  sprite_table_limit:      equ sprite_table_base + table_total_bytes + 1 ; in case we ever need to know where it ends
04A60A             0039*  
04A60A             0040*  ; pointer to top address of current record, initialized to sprite_table_base
04A60A 00 FC B7    0041*  sprite_table_pointer: dl sprite_table_base
04A60D             0042*  ; how many active sprites
04A60D 00          0043*  table_active_sprites: db 0x00
04A60E             0044*  ; flag indicating collision with screen edge
04A60E             0045*  ; uses orientation codes to specify which edge(s)
04A60E 00          0046*  sprite_screen_edge: db #00
04A60F             0047*  ; next sprite id to use
04A60F 00          0048*  sprite_next_id: db 0
04A610             0049*  
04A610             0050*  ; ######### COLLISION SPRITE PARAMETERS ##########
04A610             0051*  ; integer coordinates are all that are needed for collision calculations
04A610 00          0052*  collision_x: db 0x00
04A611 00          0053*  collision_y: db 0x00
04A612 00          0054*  collision_dim_x: db 0x00
04A613 00          0055*  collision_dim_y: db 0x00
04A614             0056*  
04A614             0057*  ; scratch variables
04A614 00          0058*  x: db 0x00 ; 8-bit signed integer
04A615 00          0059*  y: db 0x00 ; 8-bit signed integer
04A616 00 00 00    0060*  x0: dl 0x000000 ; 16.8 signed fixed place
04A619 00 00 00    0061*  y0: dl 0x000000 ; 16.8 signed fixed place
