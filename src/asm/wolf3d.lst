04657F                   0137 
04657F                   0138 ; translate camera relative x,y deltas to map x,y deltas
04657F                   0139 ; inputs: ; d = dy ; e = dx ; a = camera orientation
04657F                   0140 ; returns:	d = map_dy ; e = map_dx
04657F                   0141 trans_dx_dy:
04657F                   0142 ; modulo 4 on orientation
04657F E6 03             0143 	and 0x03
046581 28 11             0144 	jr z,@north
046583 FE 01             0145 	cp 1 ; east
046585 28 12             0146 	jr z,@east
046587 FE 02             0147 	cp 2 ; south
046589 28 12             0148 	jr z,@south
04658B FE 03             0149 	cp 3 ; west
04658D 28 13             0150 	jr z,@west
04658F                   0151 ; if none of those, return zeroes
04658F 11 00 00 00       0152 	ld de,0
046593 C9                0153 	ret
046594                   0154 @north: ; invert y axis, x unnchanged
046594 7A                0155 	ld a,d
046595 ED 44             0156 	neg
046597 57                0157 	ld d,a
046598 C9                0158 	ret
046599                   0159 @east: ; +x = +y, +y = +x
046599 7A                0160 	ld a,d
04659A 53                0161 	ld d,e
04659B 5F                0162 	ld e,a
04659C C9                0163 	ret
04659D                   0164 @south: ; invert x axis, y unchanged
04659D 7B                0165 	ld a,e
04659E ED 44             0166 	neg
0465A0 5F                0167 	ld e,a
0465A1 C9                0168 	ret
0465A2                   0169 @west: ; +x = -y, +y = -x
0465A2 7A                0170 	ld a,d ; y
0465A3 ED 44             0171 	neg ; -y
0465A5 53                0172 	ld d,e ; x
0465A6 5F                0173 	ld e,a ; -y
0465A7 7A                0174 	ld a,d ; x
0465A8 ED 44             0175 	neg ; -x
0465AA 57                0176 	ld d,a ; -x
0465AB C9                0177 	ret
