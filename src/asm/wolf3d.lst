0499CA                   0030 	include "src/asm/vdu.asm"
0499CA                   0001 ; The following is a high-level list of the VDU sequences that are supported:
0499CA                   0002 ; VDU 0: Null (no operation)
0499CA                   0003 ; VDU 1: Send next character to printer (if printer is enabled) 
0499CA                   0004 ; VDU 2: Enable printer 
0499CA                   0005 ; VDU 3: Disable printer 
0499CA                   0006 ; VDU 4: Write text at text cursor
0499CA                   0007 ; VDU 5: Write text at graphics cursor
0499CA                   0008 ; VDU 6: Enable screen (opposite of VDU 21) 
0499CA                   0009 ; VDU 7: Make a short beep (BEL)
0499CA                   0010 ; VDU 8: Move cursor back one character
0499CA                   0011 ; VDU 9: Move cursor forward one character
0499CA                   0012 ; VDU 10: Move cursor down one line
0499CA                   0013 ; VDU 11: Move cursor up one line
0499CA                   0014 ; VDU 12: Clear text area (CLS)
0499CA                   0015 ; VDU 13: Carriage return
0499CA                   0016 ; VDU 14: Page mode On *
0499CA                   0017 ; VDU 15: Page mode Off *
0499CA                   0018 ; VDU 16: Clear graphics area (CLG)
0499CA                   0019 ; VDU 17, colour: Define text colour (COLOUR)
0499CA                   0020 ; VDU 18, mode, colour: Define graphics colour (GCOL mode, colour)
0499CA                   0021 ; VDU 19, l, p, r, g, b: Define logical colour (COLOUR l, p / COLOUR l, r, g, b)
0499CA                   0022 ; VDU 20: Reset palette and text/graphics colours and drawing modes 
0499CA                   0023 ; VDU 21: Disable screen (turns of VDU command processing, except for VDU 1 and VDU 6) 
0499CA                   0024 ; VDU 22, n: Select screen mode (MODE n)
0499CA                   0025 ; VDU 23, n: Re-program display character / System Commands
0499CA                   0026 ; VDU 24, left; bottom; right; top;: Set graphics viewport **
0499CA                   0027 ; VDU 25, mode, x; y;: PLOT command
0499CA                   0028 ; VDU 26: Reset graphics and text viewports **
0499CA                   0029 ; VDU 27, char: Output character to screen 
0499CA                   0030 ; VDU 28, left, bottom, right, top: Set text viewport **
0499CA                   0031 ; VDU 29, x; y;: Set graphics origin
0499CA                   0032 ; VDU 30: Home cursor
0499CA                   0033 ; VDU 31, x, y: Move text cursor to x, y text position (TAB(x, y))
0499CA                   0034 ; VDU 127: Backspace
0499CA                   0035 
0499CA                   0036 ; VDU 0: Null (no operation)
0499CA                   0037 ;     On encountering a VDU 0 command, the VDP will do nothing.
0499CA                   0038 ;     This may be useful for padding out a VDU command sequence,
0499CA                   0039 ;     or for inserting a placeholder for a command that will be added later.
0499CA                   0040 ; inputs: none
0499CA                   0041 ; outputs: an empty byte somewhere in VDU
0499CA                   0042 ; destroys: a
0499CA                   0043 vdu_null:
0499CA AF                0044     xor a
0499CB 5B D7             0045 	rst.lil $10
0499CD C9                0046 	ret
0499CE                   0047 
0499CE                   0048 ; VDU 1: Send next character to printer (if printer is enabled) 
0499CE                   0049 ;     Ensures that the next character received by the VDP is sent through to
0499CE                   0050 ;     the printer, and not to the screen. This is useful for sending control
0499CE                   0051 ;     codes to the printer, or for sending data to the printer that is not
0499CE                   0052 ;     intended to be displayed on the screen. It allows characters that would
0499CE                   0053 ;     not otherwise normally be sent through to the printer to be sent.
0499CE                   0054 ;     If the printer has not been enabled then this command will just discard
0499CE                   0055 ;     the next byte sent to the VDP.
0499CE                   0056 ; inputs: a is the ascii code of the character to send
0499CE                   0057 ; prerequisites: "printer" must first be activated with VDU 2 (see below)
0499CE                   0058 ; outputs: a character on the serial terminal connected to the USB port
0499CE                   0059 ;           and the same character on the screen at the current text cursor location
0499CE                   0060 ; QUESTION: does it also advance the text cursor?
0499CE                   0061 ; destroys: hl, bc
0499CE                   0062 vdu_char_to_printer:
0499CE 32 DE 99 04       0063 	ld (@arg),a
0499D2 21 DD 99 04       0064 	ld hl,@cmd
0499D6 01 02 00 00       0065 	ld bc,@end-@cmd
0499DA 5B DF             0066 	rst.lil $18
0499DC C9                0067 	ret
0499DD 01                0068 @cmd: db 1
0499DE 00                0069 @arg: db 0
0499DF                   0070 @end:
0499DF                   0071 
0499DF                   0072 ; VDU 2: Enable printer 
0499DF                   0073 ;     Enables the printer.
0499DF                   0074 ;     In the context of the Agon platform, the printer is a serial
0499DF                   0075 ;     terminal that is connected to the VDPs USB port. Typically
0499DF                   0076 ;     this port is used for power, but it can also be used to send and
0499DF                   0077 ;     receive data to and from the VDP.
0499DF                   0078 ;     When the printer is enabled, the VDP will send characters it receives
0499DF                   0079 ;     to the printer as well as to the screen. It will additionally send
0499DF                   0080 ;     through control codes 8-13. To send other control codes to the printer,
0499DF                   0081 ;     use the VDU 1 command.
0499DF                   0082 ;     The VDP will not send through other control codes to the printer,
0499DF                   0083 ;     and will will not send through data it receives as part of other commands.
0499DF                   0084 vdu_enable_printer:
0499DF 3E 02             0085     ld a,2
0499E1 5B D7             0086 	rst.lil $10
0499E3 C9                0087 	ret
0499E4                   0088 
0499E4                   0089 ; VDU 3: Disable printer 
0499E4                   0090 ; inputs: none
0499E4                   0091 ; outputs: a USB port bereft of communication with the VDP
0499E4                   0092 ; destroys: a
0499E4                   0093 vdu_disable_printer:
0499E4 3E 03             0094     ld a,3
0499E6 5B D7             0095 	rst.lil $10
0499E8 C9                0096 	ret
0499E9                   0097 
0499E9                   0098 ; VDU 4: Write text at text cursor
0499E9                   0099 ;     This causes text to be written at the current text cursor position.
0499E9                   0100 ;     This is the default mode for text display.
0499E9                   0101 ;     Text is written using the current text foreground and background colours.
0499E9                   0102 ; inputs: a is the character to write to the screen
0499E9                   0103 ; prerequisites: the text cursor at the intended position on screen
0499E9                   0104 ; outputs: prints the character and moves text cursor right one position
0499E9                   0105 ; destroys: a, hl, bc
0499E9                   0106 vdu_char_to_text_cursor:
0499E9 32 F9 99 04       0107 	ld (@arg),a
0499ED 21 F8 99 04       0108 	ld hl,@cmd
0499F1 01 02 00 00       0109 	ld bc,@end-@cmd
0499F5 5B DF             0110 	rst.lil $18
0499F7 C9                0111 	ret
0499F8 04                0112 @cmd: db 4
0499F9 00                0113 @arg: db 0
0499FA                   0114 @end:
0499FA                   0115 
0499FA                   0116 ; VDU 5: Write text at graphics cursor
0499FA                   0117 ;     This causes text to be written at the current graphics cursor position.
0499FA                   0118 ;     Using this, characters may be positioned at any graphics coordinate within
0499FA                   0119 ;     the graphics viewport. This is useful for positioning text over graphics,
0499FA                   0120 ;     or for positioning text at a specific location on the screen.
0499FA                   0121 ;     Characters are plotted using the current graphics foreground colour,
0499FA                   0122 ;     using the current graphics foreground plotting mode (see VDU 18).
0499FA                   0123 ;     The character background is transparent, and will not overwrite any
