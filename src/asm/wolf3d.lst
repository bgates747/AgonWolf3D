047116 42          0059*  	ld b,d ; y
047117 0E 10       0060*  	ld c,16 ; number of cells in a column
047119 ED 4C       0061*  	mlt bc ; bc = cell_id of y,0
04711B 21 00 00 00 0062*  	ld hl,0 ; make sure uhl is zero
04711F 6B          0063*  	ld l,e ; x
