04AB4C             0350*  ; 6 	640 	240 	2 	    60hz
04AB4C             0351*  ; ** 7 	n/a 	n/a 	16 	    60hz
04AB4C             0352*  ; 8 	320 	240 	64 	    60hz
04AB4C             0353*  ; 9 	320 	240 	16 	    60hz
04AB4C             0354*  ; 10 	320 	240 	4 	    60hz
04AB4C             0355*  ; 11 	320 	240 	2 	    60hz
