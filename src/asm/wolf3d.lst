049C3D                   0030 	include "src/asm/vdu.asm"
049C3D                   0001 ; The following is a high-level list of the VDU sequences that are supported:
049C3D                   0002 ; VDU 0: Null (no operation)
049C3D                   0003 ; VDU 1: Send next character to printer (if printer is enabled) 
049C3D                   0004 ; VDU 2: Enable printer 
049C3D                   0005 ; VDU 3: Disable printer 
049C3D                   0006 ; VDU 4: Write text at text cursor
049C3D                   0007 ; VDU 5: Write text at graphics cursor
049C3D                   0008 ; VDU 6: Enable screen (opposite of VDU 21) 
049C3D                   0009 ; VDU 7: Make a short beep (BEL)
049C3D                   0010 ; VDU 8: Move cursor back one character
049C3D                   0011 ; VDU 9: Move cursor forward one character
049C3D                   0012 ; VDU 10: Move cursor down one line
049C3D                   0013 ; VDU 11: Move cursor up one line
049C3D                   0014 ; VDU 12: Clear text area (CLS)
049C3D                   0015 ; VDU 13: Carriage return
049C3D                   0016 ; VDU 14: Page mode On *
049C3D                   0017 ; VDU 15: Page mode Off *
049C3D                   0018 ; VDU 16: Clear graphics area (CLG)
049C3D                   0019 ; VDU 17, colour: Define text colour (COLOUR)
049C3D                   0020 ; VDU 18, mode, colour: Define graphics colour (GCOL mode, colour)
049C3D                   0021 ; VDU 19, l, p, r, g, b: Define logical colour (COLOUR l, p / COLOUR l, r, g, b)
049C3D                   0022 ; VDU 20: Reset palette and text/graphics colours and drawing modes 
049C3D                   0023 ; VDU 21: Disable screen (turns of VDU command processing, except for VDU 1 and VDU 6) 
049C3D                   0024 ; VDU 22, n: Select screen mode (MODE n)
049C3D                   0025 ; VDU 23, n: Re-program display character / System Commands
049C3D                   0026 ; VDU 24, left; bottom; right; top;: Set graphics viewport **
049C3D                   0027 ; VDU 25, mode, x; y;: PLOT command
049C3D                   0028 ; VDU 26: Reset graphics and text viewports **
049C3D                   0029 ; VDU 27, char: Output character to screen 
049C3D                   0030 ; VDU 28, left, bottom, right, top: Set text viewport **
049C3D                   0031 ; VDU 29, x; y;: Set graphics origin
049C3D                   0032 ; VDU 30: Home cursor
049C3D                   0033 ; VDU 31, x, y: Move text cursor to x, y text position (TAB(x, y))
049C3D                   0034 ; VDU 127: Backspace
049C3D                   0035 
049C3D                   0036 ; VDU 0: Null (no operation)
049C3D                   0037 ;     On encountering a VDU 0 command, the VDP will do nothing.
049C3D                   0038 ;     This may be useful for padding out a VDU command sequence,
049C3D                   0039 ;     or for inserting a placeholder for a command that will be added later.
049C3D                   0040 ; inputs: none
049C3D                   0041 ; outputs: an empty byte somewhere in VDU
049C3D                   0042 ; destroys: a
049C3D                   0043 vdu_null:
049C3D AF                0044     xor a
049C3E 5B D7             0045 	rst.lil $10
049C40 C9                0046 	ret
049C41                   0047 
049C41                   0048 ; VDU 1: Send next character to printer (if printer is enabled) 
049C41                   0049 ;     Ensures that the next character received by the VDP is sent through to
049C41                   0050 ;     the printer, and not to the screen. This is useful for sending control
049C41                   0051 ;     codes to the printer, or for sending data to the printer that is not
049C41                   0052 ;     intended to be displayed on the screen. It allows characters that would
049C41                   0053 ;     not otherwise normally be sent through to the printer to be sent.
049C41                   0054 ;     If the printer has not been enabled then this command will just discard
049C41                   0055 ;     the next byte sent to the VDP.
049C41                   0056 ; inputs: a is the ascii code of the character to send
049C41                   0057 ; prerequisites: "printer" must first be activated with VDU 2 (see below)
049C41                   0058 ; outputs: a character on the serial terminal connected to the USB port
049C41                   0059 ;           and the same character on the screen at the current text cursor location
049C41                   0060 ; QUESTION: does it also advance the text cursor?
049C41                   0061 ; destroys: hl, bc
049C41                   0062 vdu_char_to_printer:
049C41 32 51 9C 04       0063 	ld (@arg),a
049C45 21 50 9C 04       0064 	ld hl,@cmd
049C49 01 02 00 00       0065 	ld bc,@end-@cmd
049C4D 5B DF             0066 	rst.lil $18
049C4F C9                0067 	ret
049C50 01                0068 @cmd: db 1
049C51 00                0069 @arg: db 0
049C52                   0070 @end:
049C52                   0071 
049C52                   0072 ; VDU 2: Enable printer 
049C52                   0073 ;     Enables the printer.
049C52                   0074 ;     In the context of the Agon platform, the printer is a serial
049C52                   0075 ;     terminal that is connected to the VDPs USB port. Typically
049C52                   0076 ;     this port is used for power, but it can also be used to send and
049C52                   0077 ;     receive data to and from the VDP.
049C52                   0078 ;     When the printer is enabled, the VDP will send characters it receives
049C52                   0079 ;     to the printer as well as to the screen. It will additionally send
049C52                   0080 ;     through control codes 8-13. To send other control codes to the printer,
049C52                   0081 ;     use the VDU 1 command.
049C52                   0082 ;     The VDP will not send through other control codes to the printer,
049C52                   0083 ;     and will will not send through data it receives as part of other commands.
049C52                   0084 vdu_enable_printer:
049C52 3E 02             0085     ld a,2
049C54 5B D7             0086 	rst.lil $10
049C56 C9                0087 	ret
049C57                   0088 
049C57                   0089 ; VDU 3: Disable printer 
049C57                   0090 ; inputs: none
049C57                   0091 ; outputs: a USB port bereft of communication with the VDP
049C57                   0092 ; destroys: a
049C57                   0093 vdu_disable_printer:
049C57 3E 03             0094     ld a,3
049C59 5B D7             0095 	rst.lil $10
049C5B C9                0096 	ret
049C5C                   0097 
049C5C                   0098 ; VDU 4: Write text at text cursor
049C5C                   0099 ;     This causes text to be written at the current text cursor position.
049C5C                   0100 ;     This is the default mode for text display.
049C5C                   0101 ;     Text is written using the current text foreground and background colours.
049C5C                   0102 ; inputs: a is the character to write to the screen
049C5C                   0103 ; prerequisites: the text cursor at the intended position on screen
049C5C                   0104 ; outputs: prints the character and moves text cursor right one position
049C5C                   0105 ; destroys: a, hl, bc
049C5C                   0106 vdu_char_to_text_cursor:
049C5C 32 6C 9C 04       0107 	ld (@arg),a
049C60 21 6B 9C 04       0108 	ld hl,@cmd
049C64 01 02 00 00       0109 	ld bc,@end-@cmd
049C68 5B DF             0110 	rst.lil $18
049C6A C9                0111 	ret
049C6B 04                0112 @cmd: db 4
049C6C 00                0113 @arg: db 0
049C6D                   0114 @end:
049C6D                   0115 
049C6D                   0116 ; VDU 5: Write text at graphics cursor
049C6D                   0117 ;     This causes text to be written at the current graphics cursor position.
049C6D                   0118 ;     Using this, characters may be positioned at any graphics coordinate within
049C6D                   0119 ;     the graphics viewport. This is useful for positioning text over graphics,
049C6D                   0120 ;     or for positioning text at a specific location on the screen.
049C6D                   0121 ;     Characters are plotted using the current graphics foreground colour,
049C6D                   0122 ;     using the current graphics foreground plotting mode (see VDU 18).
049C6D                   0123 ;     The character background is transparent, and will not overwrite any
049C6D                   0124 ;     graphics that are already present at the characters location.
049C6D                   0125 ;     The exception to this is VDU 27, the delete character, which backspaces
049C6D                   0126 ;     and deletes as per its usual behaviour, but will erase using the current
049C6D                   0127 ;     graphics background colour.
049C6D                   0128 ; inputs: a is the character to write to the screen
049C6D                   0129 ; prerequisites: the graphics cursor at the intended position on screen
049C6D                   0130 ; outputs: see the name of the function
049C6D                   0131 ; destroys: a, hl, bc
049C6D                   0132 vdu_char_to_gfx_cursor:
049C6D 32 7D 9C 04       0133 	ld (@arg),a
049C71 21 7C 9C 04       0134 	ld hl,@cmd
049C75 01 02 00 00       0135 	ld bc,@end-@cmd
049C79 5B DF             0136 	rst.lil $18
049C7B C9                0137 	ret
049C7C 05                0138 @cmd: db 5
049C7D 00                0139 @arg: db 0
049C7E                   0140 @end:
049C7E                   0141 
049C7E                   0142 ; VDU 6: Enable screen (opposite of VDU 21) 
049C7E                   0143 ;     This enables the screen, and re-enables VDU command processing,
049C7E                   0144 ;     reversing the effect of VDU 21.
049C7E                   0145 ; inputs: none
049C7E                   0146 ; outputs: a functioning screen and VDU
049C7E                   0147 ; destroys: a
049C7E                   0148 vdu_enable_screen:
049C7E 3E 06             0149     ld a,6
049C80 5B D7             0150 	rst.lil $10
049C82 C9                0151 	ret
049C83                   0152 
049C83                   0153 ; PASSES
049C83                   0154 ; VDU 7: Make a short beep (BEL)
049C83                   0155 ;     Plays a short beep sound on audio channel 0. If the audio channel
049C83                   0156 ;     is already in use, or has been disabled, then this command will have no effect.
049C83                   0157 ; inputs: none
049C83                   0158 ; outputs: an unpleasant but thankfully short-lived audio tone
049C83                   0159 ; destroys: a
049C83                   0160 vdu_beep:
049C83 3E 07             0161     ld a,7
049C85 5B D7             0162 	rst.lil $10
049C87 C9                0163 	ret
049C88                   0164 
049C88                   0165 ; VDU 8: Move cursor back one character
049C88                   0166 ;     Moves the text cursor one character in the negative X direction.
049C88                   0167 ;     By default, when at the start of a line it will move to the end of
049C88                   0168 ;     the previous line (as defined by the current text viewport).
049C88                   0169 ;     If the cursor is also at the top of the screen then the viewport will scroll down.
049C88                   0170 ;     The cursor remains constrained to the current text viewport.
049C88                   0171 ;     When in VDU 5 mode and the graphics cursor is active, the viewport will not scroll.
049C88                   0172 ;     The cursor is just moved left by one character width.
049C88                   0173 ;     Further behaviour of the cursor can be controlled using the VDU 23,16 command.
049C88                   0174 ;     It should be noted that as of Console8 VDP 2.5.0, the cursor system does not
049C88                   0175 ;     support adjusting the direction of the cursors X axis, so this command
049C88                   0176 ;     will move the cursor to the left. This is likely to change in the future.
049C88                   0177 vdu_cursor_back:
049C88 3E 08             0178     ld a,8
049C8A 5B D7             0179 	rst.lil $10
049C8C C9                0180 	ret
049C8D                   0181 
049C8D                   0182 ; VDU 9: Move cursor forward one character
049C8D                   0183 vdu_cursor_forward:
049C8D 3E 09             0184     ld a,9
049C8F 5B D7             0185 	rst.lil $10
049C91 C9                0186 	ret
049C92                   0187 
049C92                   0188 ; VDU 10: Move cursor down one line
049C92                   0189 vdu_cursor_down:
049C92 3E 0A             0190     ld a,10
049C94 5B D7             0191 	rst.lil $10
049C96 C9                0192 	ret
049C97                   0193 
049C97                   0194 ; VDU 11: Move cursor up one line
049C97                   0195 vdu_cursor_up:
049C97 3E 0B             0196     ld a,11
049C99 5B D7             0197 	rst.lil $10
049C9B C9                0198 	ret
049C9C                   0199 
049C9C                   0200 ; VDU 12: Clear text area (CLS)
049C9C                   0201 vdu_cls:
049C9C 3E 0C             0202     ld a,12
049C9E 5B D7             0203 	rst.lil $10
049CA0 C9                0204 	ret
049CA1                   0205 
049CA1                   0206 ; VDU 13: Carriage return
049CA1                   0207 vdu_cr:
049CA1 3E 0D             0208     ld a,13
049CA3 5B D7             0209 	rst.lil $10
049CA5 C9                0210 	ret
049CA6                   0211 
049CA6                   0212 ; VDU 14: Page mode On *
049CA6                   0213 vdu_page_on:
049CA6 3E 0E             0214     ld a,14
049CA8 5B D7             0215 	rst.lil $10
049CAA C9                0216 	ret
049CAB                   0217 
049CAB                   0218 ; VDU 15: Page mode Off *
049CAB                   0219 vdu_page_off:
049CAB 3E 0F             0220     ld a,15
049CAD 5B D7             0221 	rst.lil $10
049CAF C9                0222 	ret
049CB0                   0223 
049CB0                   0224 ; VDU 16: Clear graphics area (CLG)
049CB0                   0225 vdu_clg:
049CB0 3E 10             0226     ld a,16
049CB2 5B D7             0227 	rst.lil $10
049CB4 C9                0228 	ret
049CB5                   0229 
049CB5                   0230 ; VDU 17, colour: Define text colour (COLOUR)
049CB5                   0231 vdu_colour_text:
049CB5 32 C5 9C 04       0232 	ld (@arg),a
049CB9 21 C4 9C 04       0233 	ld hl,@cmd
049CBD 01 02 00 00       0234 	ld bc,@end-@cmd
049CC1 5B DF             0235 	rst.lil $18
049CC3 C9                0236 	ret
049CC4 11                0237 @cmd: db 17
049CC5 00                0238 @arg: db 0
049CC6                   0239 @end:
049CC6                   0240 
049CC6                   0241 ; VDU 18, mode, colour: Set graphics colour (GCOL mode, colour)
049CC6                   0242 ; inputs: a is the plotting mode, c is the colour
049CC6                   0243 ; outputs: a VDU set to put pixels on the screen with the selected mode/colour
049CC6                   0244 vdu_gcol_fg:
049CC6                   0245 ; This command will set both the current graphics colour,
049CC6                   0246 ; and the current graphics plotting mode.
049CC6                   0247 ; As with VDU 17 the colour number will set the foreground colour
049CC6                   0248 ; if it is in the range 0-127, or the background colour if it is
049CC6                   0249 ; in the range 128-255, and will be interpreted in the same manner.
049CC6                   0250 ; Support for different plotting modes on Agon is currently very limited.
049CC6                   0251 ; The only fully supported mode is mode 0, which is the default mode.
049CC6                   0252 ; This mode will plot the given colour at the given graphics coordinate,
049CC6                   0253 ; and will overwrite any existing graphics at that coordinate. There is
049CC6                   0254 ; very limited support for mode 4, which will invert the colour of any
049CC6                   0255 ; existing graphics at the given coordinate, but this is not fully supported
049CC6                   0256 ; and may not work as expected.
049CC6                   0257 ; Support for other plotting modes, matching those provided by Acorns
049CC6                   0258 ; original VDU system, may be added in the future.
049CC6                   0259 ; This command is identical to the BASIC GCOL keyword.
049CC6 32 DB 9C 04       0260 	ld (@mode),a
049CCA 79                0261     ld a,c
049CCB 32 DC 9C 04       0262     ld (@col),a
049CCF 21 DA 9C 04       0263 	ld hl,@cmd
049CD3 01 03 00 00       0264 	ld bc,@end-@cmd
049CD7 5B DF             0265 	rst.lil $18
049CD9 C9                0266 	ret
049CDA 12                0267 @cmd: db 18
049CDB 00                0268 @mode: db 0
049CDC 00                0269 @col: db 0
049CDD                   0270 @end:
049CDD                   0271 
049CDD                   0272 vdu_gcol_bg:
049CDD 32 F4 9C 04       0273 	ld (@mode),a
049CE1 79                0274     ld a,c
049CE2 C6 80             0275     add a,128
049CE4 32 F5 9C 04       0276     ld (@col),a
049CE8 21 F3 9C 04       0277 	ld hl,@cmd
049CEC 01 03 00 00       0278 	ld bc,@end-@cmd
049CF0 5B DF             0279 	rst.lil $18
049CF2 C9                0280 	ret
049CF3 12                0281 @cmd:  db 18
049CF4 00                0282 @mode: db 0
049CF5 00                0283 @col:  db 0
049CF6                   0284 @end:
049CF6                   0285 
049CF6                   0286 ; VDU 19, l, p, r, g, b: Define logical colour (COLOUR l, p / COLOUR l, r, g, b)
049CF6                   0287 ;     This command sets the colour palette, by mapping a logical colour
049CF6                   0288 ;     to a physical colour. This is useful for defining custom colours,
049CF6                   0289 ;     or for redefining the default colours.
049CF6                   0290 ;     If the physical colour number is given as 255 then the colour will
049CF6                   0291 ;     be defined using the red, green, and blue values given. If the physical
049CF6                   0292 ;     colour number is given as any other value then the colour will be defined
049CF6                   0293 ;     using the colour palette entry given by that number, up to colour number 63.
049CF6                   0294 ;     If the physical colour is not 255 then the red, green, and blue values
049CF6                   0295 ;     must still be provided, but will be ignored.
049CF6                   0296 ;     The values for red, green and blue must be given in the range 0-255.
