049D72                   0946 
049D72                   0947 ; 2 	Not supported (Plot relative in logical inverse colour)
049D72                   0948 ; 3 	Plot relative in current background colour
049D72                   0949 dr_rel_bg: equ 3
049D72                   0950 
049D72                   0951 ; 4 	Move absolute
049D72                   0952 mv_abs: equ 4
049D72                   0953 
049D72                   0954 ; 5 	Plot absolute in current foreground colour
049D72                   0955 dr_abs_fg: equ 5
049D72                   0956 
049D72                   0957 ; 6 	Not supported (Plot absolute in logical inverse colour)
049D72                   0958 ; 7 	Plot absolute in current background colour
