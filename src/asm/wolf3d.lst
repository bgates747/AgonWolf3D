04ABC9             0978*  c_blue_dk: equ 4
04ABC9             0979*  c_magenta_dk: equ 5
04ABC9             0980*  c_cyan_dk: equ 6
04ABC9             0981*  c_grey: equ 7
04ABC9             0982*  c_grey_dk: equ 8
04ABC9             0983*  c_red: equ 9
04ABC9             0984*  c_green: equ 10
04ABC9             0985*  c_yellow: equ 11
04ABC9             0986*  c_blue: equ 12
04ABC9             0987*  c_magenta: equ 13
04ABC9             0988*  c_cyan: equ 14
