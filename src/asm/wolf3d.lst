04984E                   0001 ; ###### SPRITE TABLE FIELD INDICES ######
04984E                   0002 sprite_id:              equ 00 ; 1 byte  - unique spriteId, zero-based
04984E                   0003 sprite_obj:             equ 01 ; 1 byte  - type of sprite as defined in polys.asm, 255 is dead
04984E                   0004 sprite_health:          equ 02 ; 1 byte  - health points, signed binary, zero or negative is dead
04984E                   0005 sprite_behavior_index:  equ 03 ; 1 byte  - index of sprite's behavior subroutine in enemies.asm
04984E                   0006 sprite_x:               equ 04 ; 1 byte  - map x position
04984E                   0007 sprite_y:               equ 05 ; 1 byte  - map y position
04984E                   0008 sprite_orientation:     equ 06 ; 1 byte  - orientation
04984E                   0009 sprite_animation:       equ 07 ; 1 byte  - current animation index, zero-based
04984E                   0010 sprite_animation_timer: equ 08 ; 1 byte  - when hits zero, draw next animation frame
04984E                   0011 sprite_move_timer:      equ 09 ; 1 byte  - when zero, go to next move program, or step
04984E                   0012 sprite_move_step:       equ 10 ; 1 byte  - stage in a move program sequence, varies
04984E                   0013 sprite_points:          equ 11 ; 1 byte  - points awarded for killing this sprite type, BCD
04984E                   0014 sprite_health_damage:   equ 12 ; 1 byte  - health points deducted per successful attack on player, signed binary (negative gains health)
04984E                   0015 sprite_unassigned:      equ 13 ; 3 bytes - unassigned can be used for custom properties
04984E                   0016 sprite_record_size: equ 16 ; 16 bytes per sprite record
04984E                   0017 
04984E                   0018 
04984E                   0019 ; ###### SPRITE TABLE VARIABLES ######
04984E                   0020 ; maximum number of sprites
04984E                   0021 table_max_records:      equ 64 ; at 16 bytes per record = 1024 bytes + 7 KiB for the map is an even 8 KiB
04984E                   0022 table_total_bytes:      equ table_max_records*sprite_record_size
04984E                   0023 
04984E                   0024 ; #### THIS DEFINES THE SPACE ALLOCATED TO THE SPRITE TABLE IN EACH MAP FILE ####
04984E                   0025 sprite_table_base:       equ 0xB7FC00
04984E                   0026 sprite_table_limit:      equ sprite_table_base + table_total_bytes + 1 ; in case we ever need to know where it ends
04984E                   0027 
04984E                   0028 ; pointer to top address of current record, initialized to sprite_table_base
04984E 00 FC B7          0029 sprite_table_pointer: dl sprite_table_base
049851                   0030 ; how many active sprites
049851 00                0031 table_active_sprites: db 0x00
049852                   0032 ; flag indicating collision with screen edge
049852                   0033 ; uses orientation codes to specify which edge(s)
049852 00                0034 sprite_screen_edge: db #00
049853                   0035 ; next sprite id to use
049853 00                0036 sprite_next_id: db 0
