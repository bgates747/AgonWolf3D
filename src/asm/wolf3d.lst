04BD20             0001*  ; Table 32. Timer Control Registers
04BD20             0002*  ; this constant is the base address of the timer control registers
04BD20             0003*  ; each timer takes three bytes:
04BD20             0004*  ;   0: control register
04BD20             0005*  ;   1: low byte of timer reset value
04BD20             0006*  ;   2: high byte of timer reset value
04BD20             0007*  ; note that the value is only 8-bits, so we use in0/out0 i/o calls,
04BD20             0008*  ; which correctly force the high and upper bytes of the address bus to zero
04BD20             0009*  TMR_CTL:     equ 80h
04BD20             0010*  
04BD20             0011*  ; Timer Control Register Bit Definitions
04BD20             0012*  PRT_IRQ_0:    equ %00000000 ; The timer does not reach its end-of-count value.
04BD20             0013*                              ; This bit is reset to 0 every time the TMRx_CTL register is read.
04BD20             0014*  PRT_IRQ_1:    equ %10000000 ; The timer reaches its end-of-count value. If IRQ_EN is set to 1,
04BD20             0015*                              ; an interrupt signal is sent to the CPU. This bit remains 1 until
04BD20             0016*                              ; the TMRx_CTL register is read.
04BD20             0017*  
04BD20             0018*  IRQ_EN_0:     equ %00000000 ; Timer interrupt requests are disabled.
04BD20             0019*  IRQ_EN_1:     equ %01000000 ; Timer interrupt requests are enabled.
04BD20             0020*  
04BD20             0021*  PRT_MODE_0:   equ %00000000 ; The timer operates in SINGLE PASS mode. PRT_EN (bit 0) is reset to
04BD20             0022*                              ;  0, and counting stops when the end-of-count value is reached.
04BD20             0023*  PRT_MODE_1:   equ %00010000 ; The timer operates in CONTINUOUS mode. The timer reload value is
04BD20             0024*                              ; written to the counter when the end-of-count value is reached.
04BD20             0025*  
04BD20             0026*  ; CLK_DIV is a 2-bit mask that sets the timer input source clock divider
04BD20             0027*  CLK_DIV_256:  equ %00001100 ;
04BD20             0028*  CLK_DIV_64:   equ %00001000 ;
04BD20             0029*  CLK_DIV_16:   equ %00000100 ;
04BD20             0030*  CLK_DIV_4:    equ %00000000 ;
04BD20             0031*  
04BD20             0032*  RST_EN_0:     equ %00000000 ; The reload and restart function is disabled.
04BD20             0033*  RST_EN_1:     equ %00000010 ; The reload and restart function is enabled.
04BD20             0034*                              ; When a 1 is written to this bit, the values in the reload registers
04BD20             0035*                              ;  are loaded into the downcounter when the timer restarts. The
04BD20             0036*                              ; programmer must ensure that this bit is set to 1 each time
04BD20             0037*                              ; SINGLE-PASS mode is used.
04BD20             0038*  
04BD20             0039*  ; disable/enable the programmable reload timer
04BD20             0040*  PRT_EN_0:     equ %00000000 ;
04BD20             0041*  PRT_EN_1:     equ %00000001 ;
04BD20             0042*  
04BD20             0043*  ; Table 37. Timer Input Source Select Register
04BD20             0044*  ; Each of the 4 timers are allocated two bits of the 8-bit register
04BD20             0045*  ; in little-endian order, with TMR0 using bits 0 and 1, TMR1 using bits 2 and 3, etc.
04BD20             0046*  ;   00: System clock / CLK_DIV
04BD20             0047*  ;   01: RTC / CLK_DIV
04BD20             0048*  ;   NOTE: these are the values given in the manual, but it may be a typo
04BD20             0049*  ;   10: GPIO port B pin 1.
04BD20             0050*  ;   11: GPIO port B pin 1.
04BD20             0051*  TMR_ISS:   equ 92h ; register address
04BD20             0052*  
04BD20             0053*  ; Table 51. Real-Time Clock Control Register
04BD20             0054*  RTC_CTRL: equ EDh ; register address
04BD20             0055*  
04BD20             0056*  ; alarm interrupt disable/enable
04BD20             0057*  RTC_ALARM_0:    equ %00000000
04BD20             0058*  RTC_ALARM_1:    equ %10000000
04BD20             0059*  
04BD20             0060*  ; interrupt on alarm disable/enable
04BD20             0061*  RTC_INT_ENT_0:  equ %00000000
04BD20             0062*  RTC_INT_ENT_1:  equ %01000000
04BD20             0063*  
04BD20             0064*  RTC_BCD_EN_0:   equ %00000000   ; RTC count and alarm registers are binary
04BD20             0065*  RTC_BCD_EN_1:   equ %00100000   ; RTC count and alarm registers are BCD
04BD20             0066*  
04BD20             0067*  RTC_CLK_SEL_0:  equ %00000000   ; RTC clock source is crystal oscillator output (32768 Hz).
04BD20             0068*                                  ; On-chip 32768 Hz oscillator is enabled.
04BD20             0069*  RTC_CLK_SEL_1:  equ %00010000   ; RTC clock source is power line frequency input as set by FREQ_SEL.
04BD20             0070*                                  ; On-chip 32768 Hz oscillator is disabled.
04BD20             0071*  
04BD20             0072*  RTC_FREQ_SEL_0: equ %00000000   ; 60 Hz power line frequency.
04BD20             0073*  RTC_FREQ_SEL_1: equ %00001000   ; 50 Hz power line frequency.
04BD20             0074*  
04BD20             0075*  RTC_SLP_WAKE_0: equ %00000000   ; RTC does not generate a sleep-mode recovery reset.
04BD20             0076*  RTC_SLP_WAKE_1: equ %00000010   ; RTC generates a sleep-mode recovery reset.
04BD20             0077*  
04BD20             0078*  RTC_UNLOCK_0:   equ %00000000   ; RTC count registers are locked to prevent Write access.
04BD20             0079*                                  ; RTC counter is enabled.
04BD20             0080*  RTC_UNLOCK_1:   equ %00000001   ; RTC count registers are unlocked to allow Write access.
04BD20             0081*                                  ; RTC counter is disabled.
