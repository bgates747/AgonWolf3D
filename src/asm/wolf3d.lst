04710E 7B          0052*  	ld a,e
04710F E6 0F       0053*  	and 0x0F
047111 5F          0054*  	ld e,a
047112 7A          0055*  	ld a,d
047113 E6 0F       0056*  	and 0x0F
047115 57          0057*  	ld d,a
