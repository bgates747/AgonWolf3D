04AFA5 32 C4 AF 04 0305*      ld (@logical),a
04AFA9 79          0306*      ld a,c
04AFAA 32 C6 AF 04 0307*      ld (@red),a
04AFAE 7C          0308*      ld a,h
04AFAF 32 C7 AF 04 0309*      ld (@green),a
04AFB3 7D          0310*      ld a,l
04AFB4 32 C8 AF 04 0311*      ld (@blue),a
04AFB8 21 C3 AF 04 0312*  	ld hl,@cmd
04AFBC 01 06 00 00 0313*  	ld bc,@end-@cmd
04AFC0 5B DF       0314*  	rst.lil $18
