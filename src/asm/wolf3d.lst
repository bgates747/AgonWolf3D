04A850             0024   	include "src/asm/vdu.asm"
04A850             0001*  ; The following is a high-level list of the VDU sequences that are supported:
04A850             0002*  ; VDU 0: Null (no operation)
04A850             0003*  ; VDU 1: Send next character to printer (if printer is enabled) 
04A850             0004*  ; VDU 2: Enable printer 
04A850             0005*  ; VDU 3: Disable printer 
04A850             0006*  ; VDU 4: Write text at text cursor
04A850             0007*  ; VDU 5: Write text at graphics cursor
04A850             0008*  ; VDU 6: Enable screen (opposite of VDU 21) 
04A850             0009*  ; VDU 7: Make a short beep (BEL)
04A850             0010*  ; VDU 8: Move cursor back one character
04A850             0011*  ; VDU 9: Move cursor forward one character
04A850             0012*  ; VDU 10: Move cursor down one line
04A850             0013*  ; VDU 11: Move cursor up one line
04A850             0014*  ; VDU 12: Clear text area (CLS)
04A850             0015*  ; VDU 13: Carriage return
04A850             0016*  ; VDU 14: Page mode On *
04A850             0017*  ; VDU 15: Page mode Off *
04A850             0018*  ; VDU 16: Clear graphics area (CLG)
04A850             0019*  ; VDU 17, colour: Define text colour (COLOUR)
04A850             0020*  ; VDU 18, mode, colour: Define graphics colour (GCOL mode, colour)
04A850             0021*  ; VDU 19, l, p, r, g, b: Define logical colour (COLOUR l, p / COLOUR l, r, g, b)
04A850             0022*  ; VDU 20: Reset palette and text/graphics colours and drawing modes 
04A850             0023*  ; VDU 21: Disable screen (turns of VDU command processing, except for VDU 1 and VDU 6) 
04A850             0024*  ; VDU 22, n: Select screen mode (MODE n)
04A850             0025*  ; VDU 23, n: Re-program display character / System Commands
04A850             0026*  ; VDU 24, left; bottom; right; top;: Set graphics viewport **
04A850             0027*  ; VDU 25, mode, x; y;: PLOT command
04A850             0028*  ; VDU 26: Reset graphics and text viewports **
04A850             0029*  ; VDU 27, char: Output character to screen 
04A850             0030*  ; VDU 28, left, bottom, right, top: Set text viewport **
04A850             0031*  ; VDU 29, x; y;: Set graphics origin
04A850             0032*  ; VDU 30: Home cursor
04A850             0033*  ; VDU 31, x, y: Move text cursor to x, y text position (TAB(x, y))
04A850             0034*  ; VDU 127: Backspace
04A850             0035*  
04A850             0036*  ; VDU 0: Null (no operation)
04A850             0037*  ;     On encountering a VDU 0 command, the VDP will do nothing.
04A850             0038*  ;     This may be useful for padding out a VDU command sequence,
04A850             0039*  ;     or for inserting a placeholder for a command that will be added later.
04A850             0040*  ; inputs: none
04A850             0041*  ; outputs: an empty byte somewhere in VDU
04A850             0042*  ; destroys: a
04A850             0043*  vdu_null:
04A850 AF          0044*      xor a
04A851 5B D7       0045*  	rst.lil $10
04A853 C9          0046*  	ret
04A854             0047*  
04A854             0048*  ; VDU 1: Send next character to printer (if printer is enabled) 
04A854             0049*  ;     Ensures that the next character received by the VDP is sent through to
04A854             0050*  ;     the printer, and not to the screen. This is useful for sending control
04A854             0051*  ;     codes to the printer, or for sending data to the printer that is not
04A854             0052*  ;     intended to be displayed on the screen. It allows characters that would
04A854             0053*  ;     not otherwise normally be sent through to the printer to be sent.
04A854             0054*  ;     If the printer has not been enabled then this command will just discard
04A854             0055*  ;     the next byte sent to the VDP.
04A854             0056*  ; inputs: a is the ascii code of the character to send
04A854             0057*  ; prerequisites: "printer" must first be activated with VDU 2 (see below)
04A854             0058*  ; outputs: a character on the serial terminal connected to the USB port
04A854             0059*  ;           and the same character on the screen at the current text cursor location
04A854             0060*  ; QUESTION: does it also advance the text cursor?
04A854             0061*  ; destroys: hl, bc
04A854             0062*  vdu_char_to_printer:
04A854 32 64 A8 04 0063*  	ld (@arg),a
04A858 21 63 A8 04 0064*  	ld hl,@cmd
04A85C 01 02 00 00 0065*  	ld bc,@end-@cmd
04A860 5B DF       0066*  	rst.lil $18
04A862 C9          0067*  	ret
04A863 01          0068*  @cmd: db 1
04A864 00          0069*  @arg: db 0
04A865             0070*  @end:
04A865             0071*  
04A865             0072*  ; VDU 2: Enable printer 
04A865             0073*  ;     Enables the printer.
04A865             0074*  ;     In the context of the Agon platform, the printer is a serial
04A865             0075*  ;     terminal that is connected to the VDPs USB port. Typically
04A865             0076*  ;     this port is used for power, but it can also be used to send and
04A865             0077*  ;     receive data to and from the VDP.
04A865             0078*  ;     When the printer is enabled, the VDP will send characters it receives
04A865             0079*  ;     to the printer as well as to the screen. It will additionally send
04A865             0080*  ;     through control codes 8-13. To send other control codes to the printer,
04A865             0081*  ;     use the VDU 1 command.
04A865             0082*  ;     The VDP will not send through other control codes to the printer,
04A865             0083*  ;     and will will not send through data it receives as part of other commands.
04A865             0084*  vdu_enable_printer:
04A865 3E 02       0085*      ld a,2
04A867 5B D7       0086*  	rst.lil $10
04A869 C9          0087*  	ret
04A86A             0088*  
04A86A             0089*  ; VDU 3: Disable printer 
04A86A             0090*  ; inputs: none
04A86A             0091*  ; outputs: a USB port bereft of communication with the VDP
04A86A             0092*  ; destroys: a
04A86A             0093*  vdu_disable_printer:
04A86A 3E 03       0094*      ld a,3
04A86C 5B D7       0095*  	rst.lil $10
04A86E C9          0096*  	ret
04A86F             0097*  
04A86F             0098*  ; VDU 4: Write text at text cursor
04A86F             0099*  ;     This causes text to be written at the current text cursor position.
04A86F             0100*  ;     This is the default mode for text display.
04A86F             0101*  ;     Text is written using the current text foreground and background colours.
04A86F             0102*  ; inputs: a is the character to write to the screen
04A86F             0103*  ; prerequisites: the text cursor at the intended position on screen
04A86F             0104*  ; outputs: prints the character and moves text cursor right one position
04A86F             0105*  ; destroys: a, hl, bc
04A86F             0106*  vdu_char_to_text_cursor:
04A86F 32 7F A8 04 0107*  	ld (@arg),a
04A873 21 7E A8 04 0108*  	ld hl,@cmd
04A877 01 02 00 00 0109*  	ld bc,@end-@cmd
04A87B 5B DF       0110*  	rst.lil $18
04A87D C9          0111*  	ret
04A87E 04          0112*  @cmd: db 4
04A87F 00          0113*  @arg: db 0
04A880             0114*  @end:
04A880             0115*  
04A880             0116*  ; VDU 5: Write text at graphics cursor
04A880             0117*  ;     This causes text to be written at the current graphics cursor position.
04A880             0118*  ;     Using this, characters may be positioned at any graphics coordinate within
04A880             0119*  ;     the graphics viewport. This is useful for positioning text over graphics,
04A880             0120*  ;     or for positioning text at a specific location on the screen.
04A880             0121*  ;     Characters are plotted using the current graphics foreground colour,
04A880             0122*  ;     using the current graphics foreground plotting mode (see VDU 18).
04A880             0123*  ;     The character background is transparent, and will not overwrite any
04A880             0124*  ;     graphics that are already present at the characters location.
04A880             0125*  ;     The exception to this is VDU 27, the delete character, which backspaces
04A880             0126*  ;     and deletes as per its usual behaviour, but will erase using the current
04A880             0127*  ;     graphics background colour.
04A880             0128*  ; inputs: a is the character to write to the screen
04A880             0129*  ; prerequisites: the graphics cursor at the intended position on screen
04A880             0130*  ; outputs: see the name of the function
04A880             0131*  ; destroys: a, hl, bc
04A880             0132*  vdu_char_to_gfx_cursor:
04A880 32 90 A8 04 0133*  	ld (@arg),a
04A884 21 8F A8 04 0134*  	ld hl,@cmd
04A888 01 02 00 00 0135*  	ld bc,@end-@cmd
04A88C 5B DF       0136*  	rst.lil $18
04A88E C9          0137*  	ret
04A88F 05          0138*  @cmd: db 5
04A890 00          0139*  @arg: db 0
04A891             0140*  @end:
04A891             0141*  
04A891             0142*  ; VDU 6: Enable screen (opposite of VDU 21) 
04A891             0143*  ;     This enables the screen, and re-enables VDU command processing,
04A891             0144*  ;     reversing the effect of VDU 21.
04A891             0145*  ; inputs: none
04A891             0146*  ; outputs: a functioning screen and VDU
04A891             0147*  ; destroys: a
04A891             0148*  vdu_enable_screen:
04A891 3E 06       0149*      ld a,6
04A893 5B D7       0150*  	rst.lil $10
04A895 C9          0151*  	ret
04A896             0152*  
04A896             0153*  ; PASSES
04A896             0154*  ; VDU 7: Make a short beep (BEL)
04A896             0155*  ;     Plays a short beep sound on audio channel 0. If the audio channel
04A896             0156*  ;     is already in use, or has been disabled, then this command will have no effect.
04A896             0157*  ; inputs: none
04A896             0158*  ; outputs: an unpleasant but thankfully short-lived audio tone
04A896             0159*  ; destroys: a
04A896             0160*  vdu_beep:
04A896 3E 07       0161*      ld a,7
04A898 5B D7       0162*  	rst.lil $10
04A89A C9          0163*  	ret
04A89B             0164*  
04A89B             0165*  ; VDU 8: Move cursor back one character
04A89B             0166*  ;     Moves the text cursor one character in the negative X direction.
04A89B             0167*  ;     By default, when at the start of a line it will move to the end of
04A89B             0168*  ;     the previous line (as defined by the current text viewport).
04A89B             0169*  ;     If the cursor is also at the top of the screen then the viewport will scroll down.
04A89B             0170*  ;     The cursor remains constrained to the current text viewport.
04A89B             0171*  ;     When in VDU 5 mode and the graphics cursor is active, the viewport will not scroll.
04A89B             0172*  ;     The cursor is just moved left by one character width.
04A89B             0173*  ;     Further behaviour of the cursor can be controlled using the VDU 23,16 command.
04A89B             0174*  ;     It should be noted that as of Console8 VDP 2.5.0, the cursor system does not
04A89B             0175*  ;     support adjusting the direction of the cursors X axis, so this command
04A89B             0176*  ;     will move the cursor to the left. This is likely to change in the future.
04A89B             0177*  vdu_cursor_back:
04A89B 3E 08       0178*      ld a,8
04A89D 5B D7       0179*  	rst.lil $10
04A89F C9          0180*  	ret
