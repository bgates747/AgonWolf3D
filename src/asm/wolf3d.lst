049639             0001*  ; ###### SPRITE TABLE FIELD INDICES ######
049639             0002*  sprite_id:              equ 00 ; 1 byte  - unique spriteId, zero-based
049639             0003*  sprite_obj:             equ 01 ; 1 byte  - type of sprite as defined in polys.asm, 255 is dead
049639             0004*  sprite_health:          equ 02 ; 1 byte  - health points, signed binary, zero or negative is dead
049639             0005*  sprite_behavior_index:  equ 03 ; 1 byte  - index of sprite's behavior subroutine in enemies.asm
049639             0006*  sprite_x:               equ 04 ; 1 byte  - map x position
049639             0007*  sprite_y:               equ 05 ; 1 byte  - map y position
049639             0008*  sprite_orientation:     equ 06 ; 1 byte  - orientation
049639             0009*  sprite_animation:       equ 07 ; 1 byte  - current animation index, zero-based
049639             0010*  sprite_animation_timer: equ 08 ; 1 byte  - when hits zero, draw next animation frame
049639             0011*  sprite_move_timer:      equ 09 ; 1 byte  - when zero, go to next move program, or step
049639             0012*  sprite_move_step:       equ 10 ; 1 byte  - stage in a move program sequence, varies
049639             0013*  sprite_points:          equ 11 ; 1 byte  - points awarded for killing this sprite type, BCD
049639             0014*  sprite_health_damage:   equ 12 ; 1 byte  - health points deducted per successful attack on player, signed binary (positive gains health)
049639             0015*  sprite_unassigned:      equ 13 ; 3 bytes - unassigned can be used for custom properties
049639             0016*  sprite_record_size: equ 16 ; 16 bytes per sprite record
049639             0017*  
049639             0018*  
049639             0019*  ; ###### SPRITE TABLE VARIABLES ######
049639             0020*  ; maximum number of sprites
049639             0021*  table_max_records:      equ 64 ; at 16 bytes per record = 1024 bytes + 7 KiB for the map is an even 8 KiB
049639             0022*  table_total_bytes:      equ table_max_records*sprite_record_size
049639             0023*  
049639             0024*  ; #### THIS DEFINES THE SPACE ALLOCATED TO THE SPRITE TABLE IN EACH MAP FILE ####
049639             0025*  sprite_table_base:       equ 0xB7FC00
049639             0026*  sprite_table_limit:      equ sprite_table_base + table_total_bytes + 1 ; in case we ever need to know where it ends
049639             0027*  
049639             0028*  ; pointer to top address of current record, initialized to sprite_table_base
049639 00 FC B7    0029*  sprite_table_pointer: dl sprite_table_base
