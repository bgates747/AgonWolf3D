04A512             0358*  ; 14 	320 	200 	4 	    70hz
04A512             0359*  ; 15 	320 	200 	2 	    70hz
04A512             0360*  ; 16 	800 	600 	4 	    60hz
04A512             0361*  ; 17 	800 	600 	2 	    60hz
04A512             0362*  ; 18 	1024 	768 	2 	    60hz
04A512             0363*  ; 129 	640 	480 	4 	    60hz
04A512             0364*  ; 130 	640 	480 	2 	    60hz
04A512             0365*  ; 132 	640 	240 	16 	    60hz
04A512             0366*  ; 133 	640 	240 	4 	    60hz
04A512             0367*  ; 134 	640 	240 	2 	    60hz
04A512             0368*  ; 136 	320 	240 	64 	    60hz
04A512             0369*  ; 137 	320 	240 	16 	    60hz
04A512             0370*  ; 138 	320 	240 	4 	    60hz
04A512             0371*  ; 139 	320 	240 	2 	    60hz
