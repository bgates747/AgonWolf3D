04664B                   0158 ; clear the screen
04664B CD 46 65 04       0159     call render_background
04664F                   0160 ; get current map position and camera orientation
04664F ED 5B 28 A3 04    0161     ld de,(cur_x) ; d,e = cur_y,x
046654 CD C7 63 04       0162     call get_cell_from_coords ; ix=cell_status lut; a=obj_id, bc = cell_id
