04665C 5F                0165     ld e,a
04665D 16 06             0166     ld d,6 ; 6 bytes per orientation
04665F ED 5C             0167     mlt de ; de = orientation offset
046661 EB                0168     ex de,hl ; hl = orientation offset
046662 06 18             0169     ld b,24 ; 24 bytes per cell in cell_views lut
046664 ED 4C             0170     mlt bc ; bc = offset from base address of cell_views lut
046666 09                0171     add hl,bc ; hl = total offset from cell_views base address
