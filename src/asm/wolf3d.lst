04A5CD EB          1232*      ex de,hl ; hl = orientation offset
04A5CE 06 18       1233*      ld b,24 ; 24 bytes per cell in cell_views lut
04A5D0 ED 4C       1234*      mlt bc ; bc = offset from base address of cell_views lut
04A5D2 09          1235*      add hl,bc ; hl = total offset from cell_views base address
