04AEBD             0447*  multiPurposeDelay:
04AEBD F5          0448*      push af
04AEBE C5          0449*      push bc
04AEBF DD E5       0450*      push ix
04AEC1 47          0451*      ld b, a
04AEC2 3E 08       0452*      ld a,$08
04AEC4 5B CF       0453*      RST.LIL	08h                 ; get IX pointer to sysvars
04AEC6             0454*  
04AEC6             0455*  waitLoop:
04AEC6             0456*  
04AEC6 DD 7E 00    0457*      ld a, (ix + 0)              ; ix+0h is lowest byte of clock timer
04AEC9             0458*  
04AEC9             0459*                                  ;   we check if bit set is same as last time we checked.
04AEC9             0460*                                  ;   bit 0 - don't use
04AEC9             0461*                                  ;   bit 1 - changes 64 times per second
04AEC9             0462*                                  ;   bit 2 - changes 32 times per second
04AEC9             0463*                                  ;   bit 3 - changes 16 times per second
04AEC9             0464*  
04AEC9             0465*                                  ;   bit 4 - changes 8 times per second
04AEC9             0466*                                  ;   bit 5 - changes 4 times per second
04AEC9             0467*                                  ;   bit 6 - changes 2 times per second
04AEC9             0468*                                  ;   bit 7 - changes 1 times per second
04AEC9 A0          0469*      and b
04AECA 4F          0470*      ld c,a
04AECB 3A DC AE 04 0471*      ld a, (oldTimeStamp)
04AECF B9          0472*      cp c                        ; is A same as last value?
04AED0 28 F4       0473*      jr z, waitLoop              ; loop here if it is
04AED2 79          0474*      ld a, c
04AED3 32 DC AE 04 0475*      ld (oldTimeStamp), a        ; set new value
04AED7             0476*  
04AED7 DD E1       0477*      pop ix
04AED9 C1          0478*      pop bc
04AEDA F1          0479*      pop af
