0464C9                   0001 ; plots a bitmap character to the screen
0464C9                   0002 ; inputs: a; ascii character to plot
0464C9                   0003 ;      bc,de = x,y screen coordinates in pixels
0464C9                   0004 ;      ix pointed at the font definition lut
0464C9                   0005 ; returns: bc as the next x coordinate to plot
0464C9                   0006 ;          pretty pixels which hopefully resemble readable text
0464C9                   0007 ; destroys: lotsa stuff
0464C9                   0008 font_bmp_plot:
0464C9                   0009 ; back up bc,de
0464C9 C5                0010     push bc
0464CA D5                0011     push de
0464CB                   0012 ; get the buffer id of the character
0464CB D6 20             0013     sub 32 ; lookup table starts at ascii 32
0464CD 06 06             0014     ld b,6 ; six bytes per lookup record
0464CF 4F                0015     ld c,a
0464D0 ED 4C             0016     mlt bc
0464D2 DD 09             0017     add ix,bc ; now ix points to the character definition
0464D4 DD 27 03          0018     ld hl,(ix+3) ; hl has the buffer id
0464D7 CD 05 9E 04       0019     call vdu_buff_select
0464DB                   0020 ; plot the character
0464DB D1                0021     pop de
0464DC DD 7E 02          0022     ld a,(ix+2) ; a has the y-offset of the character
0464DF 83                0023     add a,e ; add the y-coordinate
0464E0 5F                0024     ld e,a
0464E1 3E 00             0025     ld a,0 ; can't xor it because we need carry
0464E3 8A                0026     adc a,d
0464E4 57                0027     ld d,a ; plot_y now correct for the character
0464E5 C1                0028     pop bc ; plot_x
0464E6 C5                0029     push bc ; we want it back after the plot
0464E7 CD D6 9F 04       0030     call vdu_plot_bmp
0464EB                   0031 ; now set bc to the next x coordinate
0464EB C1                0032     pop bc
0464EC DD 7E 00          0033     ld a,(ix+0) ; a has the width of the character
0464EF C6 02             0034     add a,2 ; add a little padding between characters
0464F1 81                0035     add a,c ; bump the original x-coordinate
0464F2 4F                0036     ld c,a
0464F3 3E 00             0037     ld a,0 ; can't xor it because we need carry
0464F5 88                0038     adc a,b
0464F6 47                0039     ld b,a ; plot_x now correct for the next character
0464F7 C9                0040     ret
0464F8                   0041 
0464F8                   0042 ; plots a zero-terminated string of bitmap characters to the screen
0464F8                   0043 ; inputs: hl; pointer to the string
0464F8                   0044 ;       bc,de = x,y screen coordinates in pixels
0464F8                   0045 ;       ix pointed at the font definition lut
0464F8                   0046 ; returns: bc as the next x coordinate to plot
0464F8                   0047 ;          de as the next y coordinate to plot
0464F8                   0048 ;          pretty pixels which hopefully resemble readable text
0464F8                   0049 font_bmp_print:
0464F8                   0050 @next_char:
0464F8                   0051 ; fetch the next character in the string
0464F8 7E                0052     ld a,(hl) ; a has the ascii value of the character
0464F9 FE 00             0053     cp 0 ; is it zero?
0464FB C8                0054     ret z ; if so, we're done
0464FC 23                0055     inc hl ; point to the next character
0464FD E5                0056     push hl ; save the pointer
0464FE D5                0057     push de ; save our y-coordinate
0464FF DD E5             0058     push ix ; save the font lut pointer
046501 CD C9 64 04       0059     call font_bmp_plot ; plot the character
046505 DD E1             0060     pop ix ; restore the font lut pointer
046507 D1                0061     pop de ; restore our y-coordinate
046508 E1                0062     pop hl ; restore the pointer
046509 C3 F8 64 04       0063     jp @next_char ; loop
