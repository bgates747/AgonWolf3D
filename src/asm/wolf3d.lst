04A5BE             0031   	include "src/asm/vdu.asm"
04A5BE             0001*  ; The following is a high-level list of the VDU sequences that are supported:
04A5BE             0002*  ; VDU 0: Null (no operation)
04A5BE             0003*  ; VDU 1: Send next character to printer (if printer is enabled) 
04A5BE             0004*  ; VDU 2: Enable printer 
04A5BE             0005*  ; VDU 3: Disable printer 
04A5BE             0006*  ; VDU 4: Write text at text cursor
04A5BE             0007*  ; VDU 5: Write text at graphics cursor
04A5BE             0008*  ; VDU 6: Enable screen (opposite of VDU 21) 
04A5BE             0009*  ; VDU 7: Make a short beep (BEL)
04A5BE             0010*  ; VDU 8: Move cursor back one character
04A5BE             0011*  ; VDU 9: Move cursor forward one character
04A5BE             0012*  ; VDU 10: Move cursor down one line
04A5BE             0013*  ; VDU 11: Move cursor up one line
04A5BE             0014*  ; VDU 12: Clear text area (CLS)
04A5BE             0015*  ; VDU 13: Carriage return
04A5BE             0016*  ; VDU 14: Page mode On *
04A5BE             0017*  ; VDU 15: Page mode Off *
04A5BE             0018*  ; VDU 16: Clear graphics area (CLG)
04A5BE             0019*  ; VDU 17, colour: Define text colour (COLOUR)
04A5BE             0020*  ; VDU 18, mode, colour: Define graphics colour (GCOL mode, colour)
04A5BE             0021*  ; VDU 19, l, p, r, g, b: Define logical colour (COLOUR l, p / COLOUR l, r, g, b)
04A5BE             0022*  ; VDU 20: Reset palette and text/graphics colours and drawing modes 
04A5BE             0023*  ; VDU 21: Disable screen (turns of VDU command processing, except for VDU 1 and VDU 6) 
04A5BE             0024*  ; VDU 22, n: Select screen mode (MODE n)
04A5BE             0025*  ; VDU 23, n: Re-program display character / System Commands
04A5BE             0026*  ; VDU 24, left; bottom; right; top;: Set graphics viewport **
04A5BE             0027*  ; VDU 25, mode, x; y;: PLOT command
04A5BE             0028*  ; VDU 26: Reset graphics and text viewports **
04A5BE             0029*  ; VDU 27, char: Output character to screen 
04A5BE             0030*  ; VDU 28, left, bottom, right, top: Set text viewport **
04A5BE             0031*  ; VDU 29, x; y;: Set graphics origin
04A5BE             0032*  ; VDU 30: Home cursor
04A5BE             0033*  ; VDU 31, x, y: Move text cursor to x, y text position (TAB(x, y))
04A5BE             0034*  ; VDU 127: Backspace
04A5BE             0035*  
04A5BE             0036*  ; VDU 0: Null (no operation)
04A5BE             0037*  ;     On encountering a VDU 0 command, the VDP will do nothing.
04A5BE             0038*  ;     This may be useful for padding out a VDU command sequence,
04A5BE             0039*  ;     or for inserting a placeholder for a command that will be added later.
04A5BE             0040*  ; inputs: none
04A5BE             0041*  ; outputs: an empty byte somewhere in VDU
04A5BE             0042*  ; destroys: a
04A5BE             0043*  vdu_null:
04A5BE AF          0044*      xor a
04A5BF 5B D7       0045*  	rst.lil $10
04A5C1 C9          0046*  	ret
04A5C2             0047*  
04A5C2             0048*  ; VDU 1: Send next character to printer (if printer is enabled) 
04A5C2             0049*  ;     Ensures that the next character received by the VDP is sent through to
04A5C2             0050*  ;     the printer, and not to the screen. This is useful for sending control
04A5C2             0051*  ;     codes to the printer, or for sending data to the printer that is not
04A5C2             0052*  ;     intended to be displayed on the screen. It allows characters that would
04A5C2             0053*  ;     not otherwise normally be sent through to the printer to be sent.
04A5C2             0054*  ;     If the printer has not been enabled then this command will just discard
04A5C2             0055*  ;     the next byte sent to the VDP.
04A5C2             0056*  ; inputs: a is the ascii code of the character to send
04A5C2             0057*  ; prerequisites: "printer" must first be activated with VDU 2 (see below)
04A5C2             0058*  ; outputs: a character on the serial terminal connected to the USB port
04A5C2             0059*  ;           and the same character on the screen at the current text cursor location
04A5C2             0060*  ; QUESTION: does it also advance the text cursor?
04A5C2             0061*  ; destroys: hl, bc
04A5C2             0062*  vdu_char_to_printer:
04A5C2 32 D2 A5 04 0063*  	ld (@arg),a
04A5C6 21 D1 A5 04 0064*  	ld hl,@cmd
04A5CA 01 02 00 00 0065*  	ld bc,@end-@cmd
04A5CE 5B DF       0066*  	rst.lil $18
04A5D0 C9          0067*  	ret
04A5D1 01          0068*  @cmd: db 1
04A5D2 00          0069*  @arg: db 0
04A5D3             0070*  @end:
04A5D3             0071*  
04A5D3             0072*  ; VDU 2: Enable printer 
04A5D3             0073*  ;     Enables the printer.
04A5D3             0074*  ;     In the context of the Agon platform, the printer is a serial
04A5D3             0075*  ;     terminal that is connected to the VDPs USB port. Typically
04A5D3             0076*  ;     this port is used for power, but it can also be used to send and
04A5D3             0077*  ;     receive data to and from the VDP.
04A5D3             0078*  ;     When the printer is enabled, the VDP will send characters it receives
04A5D3             0079*  ;     to the printer as well as to the screen. It will additionally send
04A5D3             0080*  ;     through control codes 8-13. To send other control codes to the printer,
04A5D3             0081*  ;     use the VDU 1 command.
04A5D3             0082*  ;     The VDP will not send through other control codes to the printer,
04A5D3             0083*  ;     and will will not send through data it receives as part of other commands.
04A5D3             0084*  vdu_enable_printer:
04A5D3 3E 02       0085*      ld a,2
04A5D5 5B D7       0086*  	rst.lil $10
04A5D7 C9          0087*  	ret
04A5D8             0088*  
04A5D8             0089*  ; VDU 3: Disable printer 
04A5D8             0090*  ; inputs: none
04A5D8             0091*  ; outputs: a USB port bereft of communication with the VDP
04A5D8             0092*  ; destroys: a
04A5D8             0093*  vdu_disable_printer:
04A5D8 3E 03       0094*      ld a,3
04A5DA 5B D7       0095*  	rst.lil $10
04A5DC C9          0096*  	ret
04A5DD             0097*  
04A5DD             0098*  ; VDU 4: Write text at text cursor
04A5DD             0099*  ;     This causes text to be written at the current text cursor position.
04A5DD             0100*  ;     This is the default mode for text display.
04A5DD             0101*  ;     Text is written using the current text foreground and background colours.
04A5DD             0102*  ; inputs: a is the character to write to the screen
04A5DD             0103*  ; prerequisites: the text cursor at the intended position on screen
04A5DD             0104*  ; outputs: prints the character and moves text cursor right one position
04A5DD             0105*  ; destroys: a, hl, bc
04A5DD             0106*  vdu_char_to_text_cursor:
04A5DD 32 ED A5 04 0107*  	ld (@arg),a
04A5E1 21 EC A5 04 0108*  	ld hl,@cmd
04A5E5 01 02 00 00 0109*  	ld bc,@end-@cmd
04A5E9 5B DF       0110*  	rst.lil $18
04A5EB C9          0111*  	ret
04A5EC 04          0112*  @cmd: db 4
04A5ED 00          0113*  @arg: db 0
04A5EE             0114*  @end:
04A5EE             0115*  
04A5EE             0116*  ; VDU 5: Write text at graphics cursor
04A5EE             0117*  ;     This causes text to be written at the current graphics cursor position.
04A5EE             0118*  ;     Using this, characters may be positioned at any graphics coordinate within
04A5EE             0119*  ;     the graphics viewport. This is useful for positioning text over graphics,
04A5EE             0120*  ;     or for positioning text at a specific location on the screen.
04A5EE             0121*  ;     Characters are plotted using the current graphics foreground colour,
04A5EE             0122*  ;     using the current graphics foreground plotting mode (see VDU 18).
04A5EE             0123*  ;     The character background is transparent, and will not overwrite any
04A5EE             0124*  ;     graphics that are already present at the characters location.
04A5EE             0125*  ;     The exception to this is VDU 27, the delete character, which backspaces
04A5EE             0126*  ;     and deletes as per its usual behaviour, but will erase using the current
04A5EE             0127*  ;     graphics background colour.
04A5EE             0128*  ; inputs: a is the character to write to the screen
04A5EE             0129*  ; prerequisites: the graphics cursor at the intended position on screen
04A5EE             0130*  ; outputs: see the name of the function
04A5EE             0131*  ; destroys: a, hl, bc
04A5EE             0132*  vdu_char_to_gfx_cursor:
04A5EE 32 FE A5 04 0133*  	ld (@arg),a
04A5F2 21 FD A5 04 0134*  	ld hl,@cmd
04A5F6 01 02 00 00 0135*  	ld bc,@end-@cmd
04A5FA 5B DF       0136*  	rst.lil $18
04A5FC C9          0137*  	ret
04A5FD 05          0138*  @cmd: db 5
04A5FE 00          0139*  @arg: db 0
04A5FF             0140*  @end:
04A5FF             0141*  
04A5FF             0142*  ; VDU 6: Enable screen (opposite of VDU 21) 
04A5FF             0143*  ;     This enables the screen, and re-enables VDU command processing,
04A5FF             0144*  ;     reversing the effect of VDU 21.
04A5FF             0145*  ; inputs: none
04A5FF             0146*  ; outputs: a functioning screen and VDU
04A5FF             0147*  ; destroys: a
04A5FF             0148*  vdu_enable_screen:
04A5FF 3E 06       0149*      ld a,6
04A601 5B D7       0150*  	rst.lil $10
04A603 C9          0151*  	ret
04A604             0152*  
04A604             0153*  ; PASSES
04A604             0154*  ; VDU 7: Make a short beep (BEL)
04A604             0155*  ;     Plays a short beep sound on audio channel 0. If the audio channel
04A604             0156*  ;     is already in use, or has been disabled, then this command will have no effect.
04A604             0157*  ; inputs: none
04A604             0158*  ; outputs: an unpleasant but thankfully short-lived audio tone
04A604             0159*  ; destroys: a
04A604             0160*  vdu_beep:
04A604 3E 07       0161*      ld a,7
04A606 5B D7       0162*  	rst.lil $10
04A608 C9          0163*  	ret
04A609             0164*  
04A609             0165*  ; VDU 8: Move cursor back one character
04A609             0166*  ;     Moves the text cursor one character in the negative X direction.
04A609             0167*  ;     By default, when at the start of a line it will move to the end of
04A609             0168*  ;     the previous line (as defined by the current text viewport).
04A609             0169*  ;     If the cursor is also at the top of the screen then the viewport will scroll down.
04A609             0170*  ;     The cursor remains constrained to the current text viewport.
04A609             0171*  ;     When in VDU 5 mode and the graphics cursor is active, the viewport will not scroll.
04A609             0172*  ;     The cursor is just moved left by one character width.
04A609             0173*  ;     Further behaviour of the cursor can be controlled using the VDU 23,16 command.
04A609             0174*  ;     It should be noted that as of Console8 VDP 2.5.0, the cursor system does not
04A609             0175*  ;     support adjusting the direction of the cursors X axis, so this command
04A609             0176*  ;     will move the cursor to the left. This is likely to change in the future.
