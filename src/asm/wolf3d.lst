04AEDD             0485*  ; inputs: hl = time to set in 1/120ths of a second; iy = pointer to 3-byte buffer holding start time, iy+3 = pointer to 3-byte buffer holding timer set value
04AEDD             0486*  ; returns: hl = current time
04AEDD             0487*  timer_set:
04AEDD FD 2F 03    0488*      ld (iy+3),hl ; set time remaining
04AEE0             0489*      MOSCALL mos_sysvars ; ix points to syvars table
04AEE0 3E 08       0001*M 			LD	A, function
04AEE2 5B CF       0002*M 			RST.LIL	08h
04AEE4 DD 27 00    0490*      ld hl,(ix+sysvar_time) ; get current time
04AEE7 FD 2F 00    0491*      ld (iy+0),hl ; set start time
04AEEA C9          0492*      ret
04AEEB             0493*  
04AEEB             0494*  ; gets time remaining on a countdown timer
04AEEB             0495*  ; inputs: iy = pointer to 3-byte buffer holding start time, iy+3 = pointer to 3-byte buffer holding timer set value
04AEEB             0496*  ; returns: hl pos = time remaining in 1/120ths of a second, hl neg = time past expiration
04AEEB             0497*  ;          sign flags: pos = time not expired, zero or neg = time expired
04AEEB             0498*  timer_get:
04AEEB             0499*      MOSCALL mos_sysvars ; ix points to syvars table
04AEEB 3E 08       0001*M 			LD	A, function
04AEED 5B CF       0002*M 			RST.LIL	08h
