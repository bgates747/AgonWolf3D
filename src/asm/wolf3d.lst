049D66             0001*  ; ###### SPRITE TABLE FIELD INDICES ######
049D66             0002*  sprite_id:              equ 00 ; 1 byte  - unique spriteId, zero-based
049D66             0003*  sprite_obj:             equ 01 ; 1 byte  - type of sprite as defined in polys.asm, 255 is dead
049D66             0004*  sprite_health:          equ 02 ; 1 byte  - health points, signed binary, zero or negative is dead
049D66             0005*  sprite_triggers_mask:   equ 03 ; 1 byte  - bitmask for tracking which sprite behaviors have been triggered
049D66             0006*  sprite_x:               equ 04 ; 1 byte  - map x position
049D66             0007*  sprite_y:               equ 05 ; 1 byte  - map y position
049D66             0008*  sprite_orientation:     equ 06 ; 1 byte  - orientation
049D66             0009*  sprite_animation:       equ 07 ; 1 byte  - current animation index, zero-based
049D66             0010*  sprite_anim_tmr: equ 08 ; 1 byte  - when hits zero, draw next animation frame
049D66             0011*  sprite_move_timer:      equ 09 ; 1 byte  - when zero, go to next move program, or step
049D66             0012*  sprite_move_step:       equ 10 ; 1 byte  - stage in a move program sequence, varies
049D66             0013*  sprite_points:          equ 11 ; 1 byte  - points awarded for killing this sprite type, BCD
049D66             0014*  sprite_health_modifier:   equ 12 ; 1 byte  - health points deducted per successful attack on player, signed binary (positive gains health)
049D66             0015*  sprite_unassigned:      equ 13 ; 3 bytes - unassigned can be used for custom properties
049D66             0016*  sprite_record_size: equ 16 ; 16 bytes per sprite record
049D66             0017*  
049D66             0018*  ; sprite_triggers_mask defs
049D66             0019*  sprite_trigger_see:     equ %00000001 ; player has been seen
049D66             0020*  sprite_trigger_use:     equ %00000010 ; player has used the sprite
049D66             0021*  sprite_trigger_hurt:    equ %00000100 ; player has hurt the sprite
049D66             0022*  sprite_trigger_kill:    equ %00001000 ; player has killed the sprite
049D66             0023*  sprite_trigger_move:    equ %00010000 ; sprite has moved
049D66             0024*  sprite_trigger_shoot:   equ %00100000 ; sprite has shot
049D66             0025*  
049D66             0026*  ; ###### SPRITE TABLE VARIABLES ######
049D66             0027*  ; maximum number of sprites
049D66             0028*  table_max_records:      equ 64 ; at 16 bytes per record = 1024 bytes + 7 KiB for the map is an even 8 KiB
